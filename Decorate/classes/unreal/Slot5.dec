Actor Unreal_EightballRocket
{
	Projectile
	Speed 18
	Radius 4
	Height 4
	Damage (85)
	+FORCERADIUSDMG
	+THRUGHOST
	DeathSound "Unreal/Flak/Explode2"
	ActiveSound "Unreal/Rocket/Fly"
	DamageType "UnrealExplosive"
	States
	{
		Spawn:
			UN00 A 0
			UN00 A 2 A_LoopActiveSound
			UN00 B 2
			Goto SpawnLoop
		SpawnLoop:
			TNT1 A 0 A_SeekerMissile(90,90,SMF_PRECISE)
			TNT1 A 0 A_SpawnItemEx("UnrealSmoke",-32*cos(pitch)-32*sin(-pitch),0,32*sin(-pitch)-32*cos(pitch),0,0,0,0,SXF_CLIENTSIDE)
			UN00 C 1 A_SpawnItemEx("UnrealSmoke",-64*cos(pitch)-32*sin(-pitch),0,64*sin(-pitch)-32*cos(pitch),0,0,0,0,SXF_CLIENTSIDE)
			Loop
		Death:
			TNT1 A 0 A_Explode(85, 200)
			TNT1 A 5 A_SpawnItemEx("Unreal_EightballExplosiveEffect",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION|SXF_CLIENTSIDE)
			Stop
	}
}

