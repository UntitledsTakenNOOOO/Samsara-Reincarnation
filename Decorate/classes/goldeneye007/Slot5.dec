actor Goldeneye_RocketCoop : Goldeneye_Rocket
{
    Species "Player"
    +THRUSPECIES
}

actor Goldeneye_RocketSoundSource : Goldeneye_Rocket { DeathSound "GoldeneyeSource/Explosion" }

actor Goldeneye_RocketCoopSoundSource : Goldeneye_RocketSoundSource
{
    Species "Player"
    +THRUSPECIES
}

actor Goldeneye_RocketSmoke
{
    Projectile
    Radius 1
    Height 1
    RenderStyle "Add"
    Alpha 0.67
    Scale 0.25
    +CLIENTSIDEONLY
    +NOBLOCKMAP
    +NOCLIP
    States
    {
      Spawn:
        GE08 A 0
        GE08 A 0 A_ChangeVelocity(0,0,0.2,CVF_REPLACE)
        GE08 A 0 A_Jump(256,"Spawn1","Spawn2","Spawn3","Spawn4")
      Spawn1:
        GE08 AAAAAAAAAAAAAA 2 A_FadeOut(0.05)
        loop

      Spawn2:
        GE08 BBBBBBBBBBBBBB 2 A_FadeOut(0.05)
        loop

      Spawn3:
        GE08 CCCCCCCCCCCCCC 2 A_FadeOut(0.05)
        loop

      Spawn4:
        GE08 DDDDDDDDDDDDDD 2 A_FadeOut(0.05)
        loop
    }
}

actor Goldeneye_ExplosionParticle
{
    Radius 1
    Height 1
    RenderStyle "Translucent"
    Alpha 0.9
    Scale 1.35
    +FORCEXYBILLBOARD
    +NOGRAVITY
    States
    {
      Spawn:
        GE05 ABCDEFGH 2 Bright
        GE05 IJKLMN 2 Bright
        stop
    }
}

actor Goldeneye_ExplosionSmoke
{
    Projectile
    Radius 1
    Height 1
    RenderStyle "Translucent"
    Alpha 0
    Scale 2.5
    +FORCEXYBILLBOARD
    +NOCLIP
    //+ROLLSPRITE // I wish I had this
    States
    {
      Spawn:
        GE04 A 0
        GE04 AAAAAAAAAAAAAAAAAAAAAAA 4 A_FadeIn(0.015)
        GE04 A 0 ACS_NamedExecuteAlways("BondSpriteEffects", 0, 2)
        GE04 A 0 A_ChangeVelocity(0,0,0.05,CVF_REPLACE)
        GE04 A 4 //A_SetRoll(roll+3,SPF_INTERPOLATE)
        wait

      Dissipate:
        GE04 "#######################" 4 A_FadeOut(0.025)
        stop
    }
}

actor Goldeneye_Debris
