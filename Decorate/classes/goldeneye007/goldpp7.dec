actor Goldfinger_GoldMagnumMagazine : Ammo
{
    Inventory.MaxAmount 6
    Tag "Golden Magnum Cylinder (Right)"
    +IGNORESKILL
}

actor Goldfinger_GoldMagnumDualMagazine : Ammo
{
    Inventory.MaxAmount 6
    Tag "Golden Magnum Cylinder (Left)"
    +IGNORESKILL
}

Actor Goldfinger_GoldMagnumPuff : Goldeneye_BulletPuff
{
    States
    {
      Spawn:
        TNT1 A 0
        TNT1 A 0 A_SetUserVar("user_damage",1000)
        Goto Super::Spawn
      XDeath:
        TNT1 A 0 A_SetUserVar("user_damage",1000)
        Goto Super::XDeath
	  Crash:
        TNT1 A 0 A_SetUserVar("user_damage",1000)
        goto Super::Crash
    }
}

