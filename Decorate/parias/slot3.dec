actor ClericSwordSlashCoop : ClericSwordSlash { Species "Player" +THRUSPECIES +DONTHARMSPECIES }

actor ClericSwordSlashTrail
{
	Height 16
	Radius 16
	Alpha 0.5
	+NOINTERACTION
	+CLIENTSIDEONLY
	States
	{
		Spawn:
			SPIR P 1 Bright A_Fadeout(0.05)
			wait
	}
}

actor ClericSwordWallPuff
{
	Height 16
	Radius 16
	+NOINTERACTION
	+CLIENTSIDEONLY
	States
	{
		Spawn:
			SPIR KLMNO 2 Bright
			Stop
	}
}

actor ClericSwordWallPuffCoop : ClericSwordWallPuff { Species "Player" +THRUSPECIES +MTHRUSPECIES }

actor ClericSwordPuff
{
	Height 16
	Radius 16
	Alpha 0.5
	+PUFFGETSOWNER
	+NOINTERACTION
	+CLIENTSIDEONLY
	+PUFFONACTORS
	States
	{
		Spawn:
			SPIR P 0 A_FaceTarget
			Goto FadeOut
		Melee:
			SPIR P 0 A_PlaySound("SpiritAttack",CHAN_BODY)
			Goto FadeOut
		XDeath:
			SPIR P 0 A_PlaySound("FighterAxeHitThing",CHAN_BODY)
			Goto FadeOut
		FadeOut:
			SPIR P 1 A_Fadeout(0.05)
			Wait
	}
}

actor ClericSwordPuffCoop : ClericSwordPuff { Species "Player" +THRUSPECIES +MTHRUSPECIES }

actor ClericSwordSlashTrail2 : ClericSwordSlashTrail { Alpha 0.45 }
actor ClericSwordSlashTrail3 : ClericSwordSlashTrail { Alpha 0.4 }
actor ClericSwordSlashTrail4 : ClericSwordSlashTrail { Alpha 0.4 }
actor ClericSwordSlashTrail5 : ClericSwordSlashTrail { Alpha 0.35 }
