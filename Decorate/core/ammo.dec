        TNT1 A 0 ACS_NamedExecuteAlways("SamsaraPickupMessages", 0, 3, 0, 0)
        TNT1 A 0 A_GiveInventory("SamsaraAmmoPickupBlend")
        TNT1 A 0 A_PlaySound("ammo/bulletsmall",CHAN_ITEM|CHAN_MAYBE_LOCAL)
