        TNT1 A 0 A_FireCustomMissile("Unreal_EightballRocket",14,false,-14)
        TNT1 A 0 A_FireCustomMissile("Unreal_EightballRocket",0,false,0,-7)
        TNT1 A 0 A_FireCustomMissile("Unreal_EightballRocket",-7,false,7)
