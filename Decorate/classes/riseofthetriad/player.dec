Actor LudicrousGibsItem : CustomInventory {
	+INVENTORY.ALWAYSPICKUP
	+INVENTORY.AUTOACTIVATE
	Inventory.Maxamount 1
	+IGNORESKILL	
	States {
		Pickup:
			TNT1 A 0 A_JumpIfInventory("RoTTClass", 1, "Success")
			Stop
		Success:
			TNT1 A 0 ACS_NamedExecuteAlways("RoTTLudicrousGibs", 0)
			Stop
	}
}

Actor SlimeToken : Inventory { Inventory.MaxAmount 1 -INVENTORY.INVBAR }

actor ROTTNetNoWeapon : Weapon
{
    Weapon.SlotNumber 1
    Weapon.SelectionOrder 1
    Tag "Net"
    +CHEATNOTWEAPON
    States
    {
      Ready:
        TNT1 A 1 A_WeaponReady
        loop

      Deselect:
        goto Ready

      Select:
        TNT1 AAAAAAAAAAAAA 1 A_Raise
        loop

      Fire:
        goto Ready
    }
}

actor ROTTPlayerPoisonPainSoundTrigger : Trigger
{
    States
    {
      Pickup:
        TNT1 A 0 A_JumpIfInventory("SamsaraIsPlayingInZandronum",1,"PickupZan")
        TNT1 A 0 A_PlaySound("*poison",CHAN_VOICE)
        stop

      PickupZan:
        TNT1 A 0 A_JumpIfInventory("RottMode",4,"PickupMale")
        TNT1 A 0 A_JumpIfInventory("RottMode",3,"PickupFemale")
        TNT1 A 0 A_JumpIfInventory("RottMode",2,"PickupFemale")
        TNT1 A 0 A_JumpIfInventory("RottMode",1,"PickupMale")
      PickupMale:
        TNT1 A 0 A_PlaySound("*poison",CHAN_VOICE)
        stop

      PickupFemale:
        TNT1 A 0 A_PlaySound("rottf/cough",CHAN_VOICE)
        stop
    }
}

actor RottDamageEnemyBeneath : Trigger
{
    States
    {
      Pickup:
        TNT1 A 0 ACS_NamedExecuteWithResult("RottDamageOnEnemy")
        stop
    }
}

actor NothingEnemyBeneath
{
    Obituary "%k stomped %o."
    +DONTSPLASH
    +NOTRIGGER
    States
    {
      Spawn:
      Crash:
        TNT1 A 1
        stop
    }
}

actor NothingEnemyBeneathCoop : NothingEnemyBeneath { +MTHRUSPECIES }
