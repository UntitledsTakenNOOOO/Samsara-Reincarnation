    Scale 0.82
    
    States
    {
      Pickup:
        TNT1 A 0 A_SetBlend("FF0000",0.15,105)
        TNT1 A 0 A_GiveInventory("PowerStrength")
        TNT1 A 0 A_GiveInventory("PowerChainsawBerserk")
        TNT1 A 0 A_GiveInventory("Doom64UnmakerArtifact")
