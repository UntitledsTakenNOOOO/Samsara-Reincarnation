actor Doom64PistolBullet : Doom64Bullet { Damage (Random(5,7)*2) }
actor Doom64ChaingunBullet : Doom64Bullet { Damage (Random(5,8)*2) }
actor Doom64ShotgunBullet : Doom64Bullet { Damage (Random(1,3)*7) }

actor Doom64Blood : Blood
{
    Scale 0.75
    States
    {
      Spawn:
      Spray:
        64BL DDD 0
        64BL D 0 A_SpawnItemEx("Switchable_FlyingBlood",0,0,0,random(-350,350)/100.00,random(-350,350)/100.00,random(500,1000)/100.00,random(-180,180),33)		
        64BL D 0 A_SpawnItemEX("Doom64Blood2",5,5,0,0,0,0,0,131,0)
        64BL D 0 A_SpawnItemEx("Doom64Blood2",-5,-5,0,0,0,0,0,131,0)
        64BL DC 9
        64BL BA 6
        Stop
    }
}

actor Doom64Blood2 : Blood
{
    Scale 0.75
    States
    {
      Spawn:
      Spray:
        64BL D 0
        64BL DC 9
        64BL BA 6
        Stop
    }
}

actor Doom64GuyBurnFlame
{
    Alpha 0.5
    xscale .5
	yscale .5
    RenderStyle Add
    +CLIENTSIDEONLY
    +NOINTERACTION
    
    States
    {
      Spawn:
        64FI ABCDE 2 bright A_FadeOut(0.025)
        loop
    }
}

actor Doom64_IHaveUnmaker : Boolean {}

Actor Doom64MonsterSet : Inventory { Inventory.MaxAmount 1 Inventory.InterHubAmount 0 }
Actor Doom64GuyMapDummy
{
	Projectile
	Damage 0
	+NOINTERACTION
	States
	{
		Spawn:
		Death:
		XDeath:
			TNT1 A 0
			TNT1 A 35 
			TNT1 A 0 A_JumpIfInTargetInventory("Doom64Mode",1,"Spawn")
			Goto Erase
		Erase:
			TNT1 A 1
			stop
	}
}

ACTOR DoomGuyScalar : CustomInventory
{
	-COUNTITEM
	+INVENTORY.ALWAYSPICKUP
	Inventory.PickupMessage ""
	Inventory.PickupSound ""
	States
	{
		Spawn:
			TNT1 A 0 
			TNT1 A 1
			Stop
		Pickup:
			TNT1 A 0 
			TNT1 A 0 A_SetScale(1.0,1.0)
			TNT1 A 1
			Stop
	}
}

ACTOR Doom64GuyScalar : CustomInventory
