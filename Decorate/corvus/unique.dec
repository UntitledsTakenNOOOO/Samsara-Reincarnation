        GPF1 ABCDEFGHIJKLMNOPQR 1 bright A_SpawnItemEx("GrimSpittleTrail2",0,0,0,random(-400,400)/100.00,random(-400,400)/100.00,random(-400,400)/100.00,random(0,360),180)
        GPF1 STUVWXYZ 2 bright
        GPF2 ABC 2 bright
        Stop	
	}
}	

