// Devastator scatter ball

actor SamsaraDoomguyStrDevastatorBall
{
	Projectile
	Radius 12
	Height 8
	Speed 30
	Damage 256
	DamageType "DoomDamageTypeDevastator"
	RenderStyle "Add"
	Alpha 0.9
	DeathSound "doomguy/stronghold/devexp"
	Decal "DoomStrDevastatorLightning"
	Obituary "%k disintegrated %o with the devastator."
	-BLOODSPLATTER
	+FORCEXYBILLBOARD
	States
	{
	Spawn:
		DVBL AB 4 Bright Light("DevBallLight")
	SpawnLoop:
		DVBL A 4 Bright Light("DevBallLight")
		TNT1 AAA 0 A_CustomMissile("SamsaraDoomguyStrLiteShot",0,0,random(0,359),CMF_TRACKOWNER)
		DVBL B 4 Bright Light("DevBallLight")
		TNT1 AAA 0 A_CustomMissile("SamsaraDoomguyStrLiteShot",0,0,random(0,359),CMF_TRACKOWNER)
		loop

	Death:
		DVBL C 5 Bright Light("DevBallBoomA")
		TNT1 A 0 A_CustomMissile("SamsaraDoomguyStrDevastatorBall2",0,0,0,CMF_AIMDIRECTION|CMF_TRACKOWNER)
		TNT1 A 0 A_CustomMissile("SamsaraDoomguyStrDevastatorBall2",0,0,18,CMF_AIMDIRECTION|CMF_TRACKOWNER)
