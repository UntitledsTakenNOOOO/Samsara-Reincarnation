        stop		

      UseRott:
        TNT1 A 0 A_FireCustomMissile("StrifeBeaconRott", 0, 0) //A_SpawnItemEx("StrifeBeaconRott",0,0,0,8,0,0,0,SXF_SETMASTER|SXF_TRANSFERTRANSLATION|SXF_TRANSFERPOINTERS)
