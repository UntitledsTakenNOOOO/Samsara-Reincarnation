ACTOR Bolognese_Guts
{
	Radius 8
	Height 12
	Speed 6
	Mass 1
	BounceFactor 0.4
	RenderStyle Translucent
	Alpha 0.9
	+BOUNCEONWALLS
	+BOUNCEONCEILINGS
	+NOBLOCKMAP
	+MISSILE
	+NOTELEPORT
	+MOVEWITHSECTOR
	+CLIENTSIDEONLY
	+FORCEXYBILLBOARD
	-EXPLODEONWATER
	//Decal BloodSplat
	Scale 1.0
	Gravity 0.4
	States
	{
	Spawn:
		XMT3 A 1
		TNT1 A 0 A_Jump(256, "Spawn1", "Spawn2", "Spawn3", "Spawn4")
	Spawn1:
		TNT1 A 0 A_SetScale(0.9, 0.8)
		Goto Live
	Spawn2:
		TNT1 A 0 A_SetScale(-1.0, 1.0)
		Goto Live
	Spawn3:
		TNT1 A 0 A_SetScale(1.2, 1.0)
		Goto Live
	Spawn4:
		TNT1 A 0 A_SetScale(-0.6, 1.0)
		Goto Live
	Live:	
		XMT3 H -1
		Loop
	Death:
		XMT3 HIJ 3
		Goto Rest
	Rest:
		TNT1 A 0 A_Jump(256, "Rest1", "Rest2")
	Rest1:
		XMT3 K 2100
		"####" K 0 A_JumpIf(ACS_NamedExecuteWithResult("SamsaraDecorate",31) == 1, "Rest1")
		"####" KKKKKKKKKK 1 A_FadeOut(0.1)
		Stop
	Rest2:
		XMT3 L 2100
		"####" L 0 A_JumpIf(ACS_NamedExecuteWithResult("SamsaraDecorate",31) == 1, "Rest2")
		"####" LLLLLLLLLL 1 A_FadeOut(0.1)
		Stop
	DoNothing:
		TNT1 A 0
		Stop
	}
}

