actor TomeOfPowerCooldown : inventory { -INVBAR +INVENTORY.UNDROPPABLE inventory.maxamount 1800 Inventory.InterHubAmount 1800 }

actor PortTome : CustomInventory
{
    tag "Tome of Power"
    inventory.maxamount 16
    inventory.usesound "heretic/itemuse"
    +INVBAR
    +inventory.undroppable
    +inventory.hubpower
    Inventory.Icon "ARTIPWBK"
	Inventory.RestrictedTo "CorvusPlayer"
    States
    {
      Use:
        TNT1 A 0 A_JumpIfInventory("PowerHereticTome",1,"UseFail")
		TNT1 A 0 A_GiveInventory("PowerHereticTome",1)
        TNT1 A 0 A_GiveInventory("TomeOfPowerCooldown",CallACS("HereticTomeChargeRate"))
        stop

      UseFail:
        TNT1 A 1
        fail
    }
}

actor PortTomeCoOp : CustomInventory
{
    tag "Tome of Power"
    inventory.maxamount 1
    inventory.usesound "heretic/itemuse"
    +INVBAR
    +inventory.undroppable
    +inventory.hubpower
    Inventory.Icon "ARTIPWBK"
	Inventory.RestrictedTo "CorvusPlayer"
    States
    {
      Pickup:
        TNT1 A 0 A_JumpIf(CallACS("TomeOfPowerCooldownReduction") == 1, 1)
        TNT1 A 0 A_RailWait
        stop

      Use:
        TNT1 A 0 A_JumpIfInventory("TomeOfPowerCooldown",1,"UseFail")
		TNT1 A 0 A_JumpIfInventory("PowerHereticTomePerma",1,"UseFail2")
        TNT1 A 1
        TNT1 A 0 A_GiveInventory("PowerHereticTome",1)
		TNT1 A 0 A_GiveInventory("TomeOfPowerCooldown",CallACS("HereticTomeChargeRate"))
        fail

      UseFail:
        TNT1 A 1
        TNT1 A 0 A_Print("$TOMECOOLDOWN")
        fail
		
      UseFail2:
        TNT1 A 1
        TNT1 A 0 A_Print("$TOMECOOLDOWN2")
        fail
    }
}

