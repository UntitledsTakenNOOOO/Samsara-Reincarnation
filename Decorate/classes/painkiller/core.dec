			TNT1 A 0 A_SetAngle(random(0,360))
			TNT1 A 0 A_Jump(256,"Spawn1","Spawn2","Spawn3")
			Goto Spawn1
		Spawn1:
			PK00 A 0
			Goto SpawnEnd
		Spawn2:
			PK00 B 0
			Goto SpawnEnd
		Spawn3:
			PK00 C 0
			Goto SpawnEnd
		SpawnEnd:
