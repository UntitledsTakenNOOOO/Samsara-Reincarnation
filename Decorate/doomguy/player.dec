actor DoomBulletPuff : BulletPuff
{
    States
    {
      Spawn:
        DPFF A 4 bright
        DPFF B 4

      Melee:
        DPFF CD 4
        Stop
    }
}

actor DoomBullet : FastProjectile
{
    Damage (Random(1,3)*5)
    Radius 1
    Height 1
    Speed 320
    Projectile
	DamageType "DoomDamageType"
    Renderstyle "Normal"
    Species "Player"
    +BLOODSPLATTER
    +THRUGHOST
    +THRUSPECIES
    +NOTIMEFREEZE
    Alpha 1.0
    Decal Bulletchip
    States
    {
      Spawn:
        TNT1 A 1
        loop

      Death:
        TNT1 A 1 A_SpawnItem("DoomBulletPuff")
        stop

      XDeath:
        TNT1 A 1
        stop
    }
}

