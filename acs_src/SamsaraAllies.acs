	for(int a = -512; a < 512; a+=64)
	{
		for(int b = -512; b < 512; b+=64)
		{
			if(random(1,1500) <= (GetCvar("samsara_randomitems")*GetCvar("samsara_randomitems"))/10)
			{	
				SpawnForced("DetermineValidSpawnTestObject",GetActorX(0)+(a*1.0),GetActorY(0)+(b*1.0),0,newtid);
				if(GetCVar("samsara_zscriptready") == 1)
					Spawn("DetermineValidSpawnItemsZS",GetActorX(0)+(a*1.0),GetActorY(0)+(b*1.0),GetActorFloorZ(newtid),0);
				else
					Spawn("DetermineValidSpawnItems",GetActorX(0)+(a*1.0),GetActorY(0)+(b*1.0),GetActorFloorZ(newtid),0);
			}
		}
	}
}

script "ItemsOnMap" Open
{
	// Unlike the misc. characters, we only need to check for the titlemap
	if(GameType () == GAME_TITLE_MAP) { terminate; }

	int clients = PlayerCount();
	int playervalue;
	
	for(int p = 0; p < clients; p++)
	{	
		if(PlayerInGame(p) == true)
		{
			pickrandomitems = 1;
		}
		else
		{
			clients += 1;
		}
	}
	
	if(pickrandomitems == true)
		ACS_NamedExecuteWithResult("DetermineValidSpawnItems",0,0,0,0);
}

Script "Samsara2689_Items" (void)
{
	int myPosX = GetActorX(0);
	int myPosY = GetActorY(0);
	int myPosZ = GetActorZ(0);
	
	int randomAngle = random(0,256);
	int randomPick = random(0,256);
	SetActorAngle(0,randomAngle<<8);
	ThrustThing(randomAngle,random(16,32),0,0);
	Delay(1);
	int myTid = UniqueTid();
	Thing_ChangeTid(0,myTid);
	int maxAttempts = 0;
	
	int newX = GetActorX(0)/1.0;
	int newY = GetActorY(0)/1.0;
	
	int floor1 = GetSectorFloorZ(0,newX-32,newY);
	int floor2 = GetSectorFloorZ(0,newX+32,newY);
	int floor3 = GetSectorFloorZ(0,newX,newY-32);
	int floor4 = GetSectorFloorZ(0,newX,newY+32);
	
	int setItemSpawns = GetCvar("samsara_currentgame");
	
	if(floor1 != floor2 || floor1 != floor3 || floor1 != floor4)
		terminate;
	
	while(maxAttempts <= 100)
	{
		switch(setItemSpawns)
		{
			case 1:
				SpawnSpotFacing("SamsaraRandomItemSpawnerDoom", 0, 0);
				break;
			case 2:
				SpawnSpotFacing("SamsaraRandomItemSpawnerChex", 0, 0);
				break;
			case 4:
				SpawnSpotFacing("SamsaraRandomItemSpawnerHeretic", 0, 0);
				break;
			case 8:
				SpawnSpotFacing("SamsaraRandomItemSpawnerHexen", 0, 0);
				break;
			case 16:
				SpawnSpotFacing("SamsaraRandomItemSpawnerStrife", 0, 0);
				break;
			case 32:
				SpawnSpotFacing("SamsaraRandomItemSpawnerDoom", 0, 0);
				break;
			case 64:
				SpawnSpotFacing("SamsaraRandomItemSpawnerHarmony", 0, 0);
				break;
			case 128:
				SpawnSpotFacing("SamsaraRandomItemSpawnerROTWB", 0, 0);
				break;
			case 256:
				SpawnSpotFacing("SamsaraRandomItemSpawnerREKKR", 0, 0);
				break;
		}
		Thing_Remove(0);
		terminate;
		
		maxAttempts++;
	}
}