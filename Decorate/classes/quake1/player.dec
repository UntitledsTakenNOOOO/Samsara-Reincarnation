    Damage (4)
    Radius 1
    Height 1
    Speed 320
    Projectile
	DamageType "QuakeExplosive"
    Renderstyle "Normal"
    Species "Player"
    +BLOODSPLATTER
    +THRUGHOST
    +THRUSPECIES
    +NOTIMEFREEZE
    Alpha 1.0
    //Decal Bulletchip
    States
    {
      Spawn:
        TNT1 A 1
        loop
      Death:
        TNT1 A 1
        TNT1 A 1 A_SpawnItem("QuakeBulletPuff")
        stop
      Crash:
      XDeath:
        TNT1 A 1
        stop
    }
}

actor QuakeBulletSpectral: QuakeBullet
{
    -THRUGHOST
    DamageType "Spectral"
}

Actor QuakeQuadBullet : QuakeBullet
{
	DamageType "QuakeQuadded"
}	

