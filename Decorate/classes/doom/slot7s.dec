        stop

      // Powered
      PickupAttackPowered:
        TNT1 A 0 A_JumpIfInventory("SPModeOn",1,"PickupAttackPoweredCoop")
        TNT1 A 0 A_JumpIfInventory("CoopModeOn",1,"PickupAttackPoweredCoop")
      PickupAttackPoweredDM:
        TNT1 A 0 A_FireBullets(4,4,4,4,"SamsaraDoomguyStrDevPuff",FBF_NOFLASH,random(256,512))
        stop

      PickupAttackPoweredCoop:
        TNT1 A 0 A_FireBullets(4,4,4,4,"SamsaraDoomguyStrDevPuffCoop",FBF_NOFLASH,random(256,512))
        stop
    }
}

// Ranged attack handler

actor SamsaraDoomguyStrDevastatorRangedAttackHandler : Trigger
{
    States
    {
      Pickup:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"PickupAttack")
        TNT1 A 0 A_JumpIfInventory("Cell",60,1)
        stop
        TNT1 A 0 A_TakeInventory("Cell",60)
        goto PickupAttack

      // Attack stuff
      PickupAttack:
        TNT1 A 0 A_PlaySound("doomguy/stronghold/devfir",CHAN_WEAPON)
        TNT1 A 0 A_JumpIfInventory("PowerSamsaraDoomguyStrWeaponPower",1,"PickupAttackPowered")
        TNT1 A 0 A_JumpIfInventory("SamsaraHasPermaWeaponLevel2",1,"PickupAttackPowered")
      // Unpowered
      PickupAttackUnpowered:
        TNT1 A 0 A_JumpIfInventory("SamsaraDoomguyStrDevastatorHeldCheck",1,"PickupAttackUnpoweredHold")
      // Fire
      PickupAttackUnpoweredFire:
        TNT1 A 0 A_JumpIfInventory("SPModeOn",1,"PickupAttackUnpoweredFireCoop")
        TNT1 A 0 A_JumpIfInventory("CoopModeOn",1,"PickupAttackUnpoweredFireCoop")
      PickupAttackUnpoweredFireDM:
        TNT1 A 0 A_FireCustomMissile("SamsaraDoomguyStrDevastatorBall",0,false)
        stop

      PickupAttackUnpoweredFireCoop:
        TNT1 A 0 A_FireCustomMissile("SamsaraDoomguyStrDevastatorBallCoop",0,false)
        stop

      // Hold
      PickupAttackUnpoweredHold:
        TNT1 A 0 A_JumpIfInventory("SPModeOn",1,"PickupAttackUnpoweredHoldCoop")
        TNT1 A 0 A_JumpIfInventory("CoopModeOn",1,"PickupAttackUnpoweredHoldCoop")
      PickupAttackUnpoweredHoldDM:
        TNT1 A 0 A_FireCustomMissile("SamsaraDoomguyStrDevastatorBall1",0,false)
        stop

      PickupAttackUnpoweredHoldCoop:
        TNT1 A 0 A_FireCustomMissile("SamsaraDoomguyStrDevastatorBall1Coop",0,false)
        stop

      // Powered
      PickupAttackPowered:
      // Puffs
      PickupAttackPowered1:
        TNT1 A 0 A_JumpIfInventory("SamsaraDoomguyStrDevastatorHeldCheck",1,1)
        goto PickupAttackPowered2
        TNT1 A 0 A_JumpIfInventory("SPModeOn",1,"PickupAttackPowered1Coop")
        TNT1 A 0 A_JumpIfInventory("CoopModeOn",1,"PickupAttackPowered1Coop")
      PickupAttackPowered1DM:
        TNT1 A 0 A_FireBullets(25,25,250,50,"SamsaraDoomguyStrDevPuff",FBF_NOFLASH)
        goto PickupAttackPowered2

      PickupAttackPowered1Coop:
        TNT1 A 0 A_FireBullets(25,25,250,50,"SamsaraDoomguyStrDevPuffCoop",FBF_NOFLASH)
        goto PickupAttackPowered2

      // Damaging trail projectile + main projectile
      PickupAttackPowered2:
        TNT1 A 0 A_FireCustomMissile("SamsaraDoomguyStrDevastatorCannonProjectile",0,false)
        TNT1 A 0 A_JumpIfInventory("SPModeOn",1,"PickupAttackPowered2Coop")
        TNT1 A 0 A_JumpIfInventory("CoopModeOn",1,"PickupAttackPowered2Coop")
      PickupAttackPowered2DM:
        TNT1 A 0 A_FireCustomMissile("SamsaraDoomguyStrDevastatorCannonProjectile2",0,false)
        stop

      PickupAttackPowered2Coop:
        TNT1 A 0 A_FireCustomMissile("SamsaraDoomguyStrDevastatorCannonProjectile2Coop",0,false)
        stop
    }
}

