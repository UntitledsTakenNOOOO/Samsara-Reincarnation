actor PDWTAKNapalmDebris
{
  health 5
  radius 1
  height 1
  Speed 10
  Scale 0.2
  Alpha 0.5
  PROJECTILE
  +NOBLOCKMAP
  +DONTSPLASH
  +RANDOMIZE
  +NOBLOCKMONST
  +THRUACTORS
  -NOGRAVITY
  -SOLID
  +CLIENTSIDEONLY  
  +NOINTERACTION  
  +WINDTHRUST
  states
  {
  Spawn:
	TNT1 A 0
    FSPK A 0 bright A_Jump(192,3,4,6,8)
    FSPK A 3 bright
    FSPK A 50 bright A_LowGravity
    Stop
    FSPK B 3 bright
    FSPK B 50 bright A_LowGravity
    Stop
    FSPK C 3 bright
    FSPK C 50 bright A_LowGravity
    Stop
    FSPK D 3 bright
    FSPK D 50 bright A_LowGravity
    Stop
    FSPK E 3 bright
    FSPK E 50 bright A_LowGravity
    Stop
    FSPK F 3 bright
    FSPK F 50 bright A_LowGravity
    Stop	
  Death:
    FSPK A 1
    Stop
  }
}

