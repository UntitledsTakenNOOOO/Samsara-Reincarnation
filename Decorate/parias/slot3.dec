actor "Timon's Axe" : Weapon // FWeapAxe
{
	Weapon.SelectionOrder 1500
	Weapon.SlotNumber 3
	Weapon.AmmoUse1 0//3
	Weapon.AmmoGive1 25
	Weapon.KickBack 150
	Weapon.YAdjust -12
	DamageType "TimonsAxe"
	Weapon.AmmoType1 "Clip"
	Inventory.PickupMessage "You got Timon's Axe!"
	Inventory.PickupSound "hexen/weaponget"
	Obituary "$SAMSARA_HEXEN_OB_SLOT3"
	Inventory.RestrictedTo "HexenPlayer"
	+WEAPON.AMMO_OPTIONAL
	+WEAPON.MELEEWEAPON
	+INVENTORY.UNDROPPABLE
	+PIERCEARMOR
	Tag "Bishop's Shortsword"
	States
	{
	  Spawn:
		WFAX A -1
		Stop

	  Select:
		TNT1 A 0 A_JumpIfInventory("HexenClassMode", 2, "SelectFighter")
		TNT1 A 0 A_JumpIfInventory("HexenClassMode", 1, "SelectMage")
		H119 A 0 A_JumpIfInventory("Clip", 4, "SelectClericGlow")
		H119 I 1 A_Raise
		Loop
	  SelectFighter:
		FAXE A 0 A_JumpIfInventory("Clip", 3, "SelectFighterGlow")
		FAXE A 1 A_Raise
		Loop
	  SelectMage:
		H113 AAAABBBBCCCC 1 A_Raise
		Loop

	  Deselect:
		TNT1 A 0 A_JumpIfInventory("HexenClassMode", 2, "DeselectFighter")
		TNT1 A 0 A_JumpIfInventory("HexenClassMode", 1, "DeselectMage")
		H119 A 0 A_JumpIfInventory("Clip", 4, "DeselectClericGlow")
		H119 I 1 A_Lower
		Loop
	  DeselectFighter:
		FAXE A 0 A_JumpIfInventory("Clip", 3, "DeselectFighterGlow")
		FAXE A 1 A_Lower
		Loop
	  DeselectMage:
		H113 AAAABBBBCCCC 1 A_Lower
		Loop

	  Ready:
		TNT1 A 0 A_JumpIfInventory("HexenClassMode", 2, "ReadyFighter")
		TNT1 A 0 A_JumpIfInventory("HexenClassMode", 1, "ReadyMage")
		H119 A 0 A_JumpIfInventory("Clip", 4, "ReadyClericGlow")
		H119 I 1 A_WeaponReady
		Loop

	  ReadyFighter:
		FAXE A 0 A_JumpIfInventory ("Clip", 3, "ReadyFighterGlow")
		FAXE A 1 A_WeaponReady
		Goto Ready

	  ReadyMage:
		H113 AAAABBBBCCCC 1 A_WeaponReady
		Goto Ready

	  SelectClericGlow:
		H119 AAABBB 1 A_Raise
		Loop

	  DeselectClericGlow:
		H119 AAABBB 1 A_Lower
		Loop

	  ReadyClericGlow:
		TNT1 A 0 A_JumpIfInventory("HexenClassMode", 2, "ReadyFighter")
		TNT1 A 0 A_JumpIfInventory("HexenClassMode", 1, "ReadyMage")
		H119 A 0 A_JumpIfNoAmmo("Ready")
		H119 A 3 A_WeaponReady
		H119 B 0 A_JumpIfNoAmmo("Ready")
		H119 B 3 A_WeaponReady
		Loop

	  SelectFighterGlow:
		FAXE L 0 A_JumpIfNoAmmo("Select")
		FAXE L 1 A_Raise
		FAXE L 0 A_JumpIfNoAmmo("Select")
		FAXE L 1 A_Raise
		FAXE L 0 A_JumpIfNoAmmo("Select")
		FAXE L 1 A_Raise
		FAXE M 0 A_JumpIfNoAmmo("Select")
		FAXE M 1 A_Raise
		FAXE M 0 A_JumpIfNoAmmo("Select")
		FAXE M 1 A_Raise
		FAXE M 0 A_JumpIfNoAmmo("Select")
		FAXE M 1 A_Raise
		loop

	  DeselectFighterGlow:
		FAXE L 1 A_Lower
		Loop

	  ReadyFighterGlow:
		FAXE L 1 A_WeaponReady
		FAXE L 0 A_JumpIfNoAmmo("Ready")
		FAXE L 1 A_WeaponReady
		FAXE L 0 A_JumpIfNoAmmo("Ready")
		FAXE L 1 A_WeaponReady
		FAXE L 0 A_JumpIfNoAmmo("Ready")
		FAXE M 1 A_WeaponReady
		FAXE M 0 A_JumpIfNoAmmo("Ready")
		FAXE M 1 A_WeaponReady
		FAXE M 0 A_JumpIfNoAmmo("Ready")
		FAXE M 1 A_WeaponReady
		FAXE M 0 A_JumpIfNoAmmo("Ready")
		Goto Ready
	  
	  Fire:
		TNT1 A 0 A_JumpIfInventory("HexenClassMode", 2, "FireFighter")
		TNT1 A 0 A_JumpIfInventory("HexenClassMode", 1, "FireMage")
		H119 J 5 A_JumpIfInventory("Clip", 4, "FireClericGlow")
		H119 K 5 A_PlaySound("Hexen/BishopSword/Swing",CHAN_WEAPON)
		H119 L 5
		H119 M 0 A_JumpIfInventory("CoopModeOn",1,"FireClericCoop")
		H119 M 0 A_FireBullets(-32,-16,-1,5,"ClericSwordPuffNormal",FBF_NORANDOMPUFFZ|FBF_EXPLICITANGLE,120)
		H119 M 0 A_FireBullets(-16,-12,-1,5,"ClericSwordPuffNormal",FBF_NORANDOMPUFFZ|FBF_EXPLICITANGLE,120)
		H119 M 0 A_FireBullets(0,-8,-1,5,"ClericSwordPuffNormal",FBF_NORANDOMPUFFZ|FBF_EXPLICITANGLE,120)
		H119 M 0 A_FireBullets(16,-12,-1,5,"ClericSwordPuffNormal",FBF_NORANDOMPUFFZ|FBF_EXPLICITANGLE,120)
		H119 M 5 A_FireBullets(32,0,-1,5,"ClericSwordPuffNormal",FBF_NORANDOMPUFFZ|FBF_EXPLICITANGLE,120)
		Goto FireClericFinish
	  FireClericCoop:
		H119 M 0 A_FireBullets(-32,-16,-1,5,"ClericSwordPuffNormalCoop",FBF_NORANDOMPUFFZ|FBF_EXPLICITANGLE,120)
		H119 M 0 A_FireBullets(-16,-12,-1,5,"ClericSwordPuffNormalCoop",FBF_NORANDOMPUFFZ|FBF_EXPLICITANGLE,120)
		H119 M 0 A_FireBullets(0,-8,-1,5,"ClericSwordPuffNormalCoop",FBF_NORANDOMPUFFZ|FBF_EXPLICITANGLE,120)
		H119 M 0 A_FireBullets(16,-12,-1,5,"ClericSwordPuffNormalCoop",FBF_NORANDOMPUFFZ|FBF_EXPLICITANGLE,120)
		H119 M 5 A_FireBullets(32,0,-1,5,"ClericSwordPuffNormalCoop",FBF_NORANDOMPUFFZ|FBF_EXPLICITANGLE,120)
		Goto FireClericFinish
	  FireClericFinish:
		H119 N 5
		Goto Ready

	  FireClericGlow:
		H119 C 5 A_PlaySound("Hexen/BishopSword/SwingHoly",CHAN_WEAPON)
		H119 DE 5
		H119 F 0 A_JumpIfInventory("CoopModeOn",1,"FireClericGlowCoop")
		H119 F 0 A_FireBullets(-32,-16,-1,10,"ClericSwordPuff",FBF_NORANDOMPUFFZ|FBF_EXPLICITANGLE,120)
		H119 F 0 A_FireBullets(-16,-12,-1,10,"ClericSwordPuff",FBF_NORANDOMPUFFZ|FBF_EXPLICITANGLE,120)
		H119 F 0 A_FireBullets(0,-8,-1,10,"ClericSwordWallPuff",FBF_NORANDOMPUFFZ|FBF_EXPLICITANGLE,120)
		H119 F 0 A_FireBullets(16,-12,-1,10,"ClericSwordPuff",FBF_NORANDOMPUFFZ|FBF_EXPLICITANGLE,120)
		H119 F 0 A_FireBullets(32,0,-1,10,"ClericSwordPuff",FBF_NORANDOMPUFFZ|FBF_EXPLICITANGLE,120)
		H119 F 0 A_FireCustomMissile("ClericSwordSlash",0,0)
		H119 F 0 A_FireCustomMissile("ClericSwordSlash",0,0,-16)
		H119 F 0 A_FireCustomMissile("ClericSwordSlash",0,0,16)
		Goto FireClericGlowFinish
	  FireClericGlowCoop:
		H119 F 0 A_FireBullets(-32,-16,-1,10,"ClericSwordPuffCoop",FBF_NORANDOMPUFFZ|FBF_EXPLICITANGLE,120)
		H119 F 0 A_FireBullets(-16,-12,-1,10,"ClericSwordPuffCoop",FBF_NORANDOMPUFFZ|FBF_EXPLICITANGLE,120)
		H119 F 0 A_FireBullets(0,-8,-1,10,"ClericSwordWallPuffCoop",FBF_NORANDOMPUFFZ|FBF_EXPLICITANGLE,120)
		H119 F 0 A_FireBullets(16,-12,-1,10,"ClericSwordPuffCoop",FBF_NORANDOMPUFFZ|FBF_EXPLICITANGLE,120)
		H119 F 0 A_FireBullets(32,0,-1,10,"ClericSwordPuffCoop",FBF_NORANDOMPUFFZ|FBF_EXPLICITANGLE,120)
		H119 F 0 A_FireCustomMissile("ClericSwordSlashCoop",0,0)
		H119 F 0 A_FireCustomMissile("ClericSwordSlashCoop",0,0,-16)
		H119 F 0 A_FireCustomMissile("ClericSwordSlashCoop",0,0,16)
		Goto FireClericGlowFinish
	  FireClericGlowFinish:
		H119 F 5 A_TakeInventory("Clip", 4, TIF_NOTAKEINFINITE)
		H119 G 5
		Goto Ready

	  FireMage:
		H113 DE 3
		H113 F 3 A_JumpIfInventory("Clip", 3, 1)
		Goto SwitchWeapon
		H113 G 5
		H113 H 3 A_PlayWeaponSound("FireDemonAttack")
		H113 H 0 A_JumpIfInventory("CoopModeOn", 1, "CoopFireMage")
		H113 H 0 A_FireCustomMissile("MWeapFire_Missile", 0, false ,-4, 8, 0) // This was removed for damage reasons.
		H113 H 0 A_FireCustomMissile("MWeapFire_Missile", 3, false ,-4, 8,-1) // TODO: make these tighter to compensate(?)
		H113 H 0 A_FireCustomMissile("MWeapFire_Missile", 0, false ,-4, 8, -3)
		H113 H 0 A_FireCustomMissile("MWeapFire_Missile",-3, false ,-4, 8, -1)
		H113 H 0 A_FireCustomMissile("MWeapFire_Missile", 1, false ,-4, 8, 2)
		H113 H 0 A_FireCustomMissile("MWeapFire_Missile",-1, false,-4, 8, 2)
		Goto FireMageEnd
	  CoopFireMage:
		H113 H 0 A_FireCustomMissile("MWeapFire_MissileCoop", 0, false ,-4, 8, 0)
		H113 H 0 A_FireCustomMissile("MWeapFire_MissileCoop", 3, false ,-4, 8,-1) // TODO: make these tighter to compensate(?)
		H113 H 0 A_FireCustomMissile("MWeapFire_MissileCoop", 0, false ,-4, 8, -3)
		H113 H 0 A_FireCustomMissile("MWeapFire_MissileCoop",-3, false ,-4, 8, -1)
		H113 H 0 A_FireCustomMissile("MWeapFire_MissileCoop", 1, false ,-4, 8, 2)
		H113 H 0 A_FireCustomMissile("MWeapFire_MissileCoop",-1, TRUE,-4, 8, 2)
		Goto FireMageEnd
	  FireMageEnd:
		H113 F 3 A_TakeInventory("Clip", 3, TIF_NOTAKEINFINITE)
		H113 ED 3
		H113 A 2 A_ReFire("FireMage")
		Goto Ready

 /*	 SPFire:
		H119 E 0 A_PlayWeaponSound("Hexen/WandOfEmbers/Fire")
		H119 F 0 A_FireCustomMissile("WandOfEmbersBall_2", Random(-7, -4), 0, 0, Random(-3,8))
		H119 F 0 A_FireCustomMissile("WandOfEmbersBall_2", Random(8, 3), 0, 0, Random(-2, 9))
		H119 F 0 A_FireCustomMissile("WandOfEmbersBall_2", Random(-10, -2), 0, 0, Random(-1,12))
		H119 F 0 A_FireCustomMissile("WandOfEmbersBall_2", Random(10, 2), 0, 0, Random(-1, 12))		
		H119 F 0 A_FireCustomMissile("WandOfEmbersBall_2", 0, 0, 0, 0)
		H119 F 0 A_TakeInventory("Clip", 4, TIF_NOTAKEINFINITE)
		H119 FG 6 Bright
		H119 D 7
		H119 C 5 A_ReFire
		H119 B 6
		Goto Ready*/

	  FireFighter:
		H104 B 4 Offset (15, 32) A_JumpIfInventory("Clip",3,"FireFighterGlow")//A_FAxeCheckAtk
		H104 C 3 Offset (15, 32)
		H104 D 2 Offset (15, 32)
		TNT1 A 0 A_JumpIfInventory("CoopModeOn",1,"CoopFireFighter")
		TNT1 A 0 A_CustomPunch(random(50,78),1,1,"AxePuff2",144)
		TNT1 A 0 A_JumpIfInventory("SPModeOn",1,"SPFireFighter")
		Goto FireFighterEnd
	  SPFireFighter:
		TNT1 A 0 A_CustomPunch(2,0,0,"BlankPuff4",144)
		TNT1 A 0 A_CustomPunch(3,0,0,"BlankPuff4",84)
		Goto FireFighterEnd
	  CoopFireFighter:
	  	TNT1 A 0 A_CustomPunch(random(50,78),1,1,"AxePuff2Coop",144)
		TNT1 A 0 A_CustomPunch(2,0,0,"BlankPuff4Coop",144)
		TNT1 A 0 A_CustomPunch(3,0,0,"BlankPuff4Coop",84)
		Goto FireFighterEnd

	  FireFighterEnd:
		H104 D 1 Offset (-5, 70)
		H104 D 2 Offset (-25, 90)
		Goto FireFighter2

	  FireFighter2:
		FAXE E 1 Offset (15, 32)
		FAXE E 2 Offset (10, 54)
		FAXE E 7 Offset (10, 150)
		FAXE A 1 Offset (0, 60) A_ReFire
		FAXE A 1 Offset (0, 52)
		FAXE A 1 Offset (0, 44)
		FAXE A 1 Offset (0, 36)
		FAXE A 1
		Goto Ready

	  FireFighterGlow:
		H104 N 4 BRIGHT Offset (15, 32)
		H104 O 3 BRIGHT Offset (15, 32)
		H104 P 2 BRIGHT Offset (15, 32)
		TNT1 A 0 A_JumpIfInventory("CoopModeOn",1,"CoopFireFighterGlow")
		TNT1 A 0 A_CustomPunch(random(120,195),1,1,"AxePuffGlow2",144)
		TNT1 A 0 A_JumpIfInventory("SPModeOn",1,"SPFireFighterGlow")
		Goto FireFighterGlowEnd
	  SPFireFighterGlow:
	  	TNT1 A 0 A_CustomPunch(3,0,0,"BlankPuff4",144)
		TNT1 AA 0 A_CustomPunch(5,0,0,"BlankPuff4",84)
		Goto FireFighterGlowEnd
	  CoopFireFighterGlow:
		TNT1 A 0 A_CustomPunch(random(120,195),1,1,"AxePuffGlow2Coop",144)
		TNT1 A 0 A_CustomPunch(3,0,0,"BlankPuff4Coop",144)
		TNT1 AA 0 A_CustomPunch(5,0,0,"BlankPuff4Coop",84)
		Goto FireFighterGlowEnd
	  FireFighterGlowEnd:
		H104 P 1 BRIGHT Offset (-5, 70)
		H104 P 2 BRIGHT Offset (-25, 90)
		FAXE Q 1 BRIGHT Offset (15, 32) A_JumpIfNoAmmo("FireFighter2")
		FAXE Q 2 BRIGHT Offset (10, 54)
		FAXE Q 7 BRIGHT Offset (10, 150)
		FAXE A 1 BRIGHT Offset (0, 60) A_ReFire
		FAXE A 1 BRIGHT Offset (0, 52)
		FAXE A 1 BRIGHT Offset (0, 44)
		FAXE A 1 BRIGHT Offset (0, 36)
		FAXE A 1 BRIGHT
		Goto ReadyFighterGlow

	  SwitchWeapon:
		TNT1 A 0 ACS_NamedExecuteWithResult("Hexen_SwitchPriority")
		TNT1 A 0
		Goto Ready
	}
}

