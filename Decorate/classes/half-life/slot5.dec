actor "Spore Launcher" : HLWeapon
{
    Inventory.PickupMessage "You got the Spore Launcher!"
    Inventory.Icon "HW50Y0"
    Weapon.SlotNumber 5
    Weapon.SelectionOrder 1350
    Weapon.AmmoType1 "HLSporeCounter"
    Weapon.AmmoUse1 1
    Weapon.AmmoType2 "RocketAmmo"
    //Weapon.AmmoGive2 10
    +EXPLOSIVE
    Tag "Spore Launcher"
    States
    {
      Spawn:
        OW50 ABCDEFGHIJKLMNOPQRSTU 3
        loop

      // Gordon Freeman
      ReadyActual:
        0P00 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"ReadyActualReloadable")
      ReadyActualNonReloadable:
        0P00 A 0 A_JumpIfInventory("HalfLifeOpposingForce",1,"ReadyActualNonReloadableShephard")
        0P00 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady
        0P01 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady
        0P02 ABCDEFGH 1 A_WeaponReady
        goto ReadyActualJump

      ReadyActualReloadable:
        0P00 A 0 A_JumpIfInventory("HalfLifeOpposingForce",1,"ReadyActualReloadableShephard")
        0P00 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady(WRF_ALLOWRELOAD)
        0P01 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady(WRF_ALLOWRELOAD)
        0P02 ABCDEFGH 1 A_WeaponReady(WRF_ALLOWRELOAD)
        goto ReadyActualJump

      ReadyActualJump:
        0P00 A 0 A_Jump(4,"ReadyActualAmmoCheck")
        0P00 A 0 A_Jump(3,"ReadyActualAmmoCheck")
        0P00 A 0 A_Jump(2,"ReadyActualAmmoCheck")
        0P00 A 0 A_Jump(1,"ReadyActualIdle1","ReadyActualIdle2")
        goto ReadyActualAmmoCheck

      ReadyActualIdle1:
        0P00 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"ReadyActualIdle1Reloadable")
      ReadyActualIdle1NonReloadable:
        0P00 A 0 A_JumpIfInventory("HalfLifeOpposingForce",1,"ReadyActualIdle1NonReloadableShephard")
        0P00 A 1 A_WeaponReady
        0P17 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady
        0P18 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady
        0P19 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady
        0P20 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady
        0P21 ABCDEFGHIJKLMNOP 1 A_WeaponReady
        goto ReadyActual

      ReadyActualIdle1Reloadable:
        0P00 A 0 A_JumpIfInventory("HalfLifeOpposingForce",1,"ReadyActualIdle1ReloadableShephard")
        0P00 A 1 A_WeaponReady(WRF_ALLOWRELOAD)
        0P17 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady(WRF_ALLOWRELOAD)
        0P18 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady(WRF_ALLOWRELOAD)
        0P19 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady(WRF_ALLOWRELOAD)
        0P20 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady(WRF_ALLOWRELOAD)
        0P21 ABCDEFGHIJKLMNOP 1 A_WeaponReady(WRF_ALLOWRELOAD)
        goto ReadyActual

      ReadyActualIdle2: // commence petting
        0P00 A 0 A_PlaySound("HLSporeLauncher/Pet")
        0P00 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"ReadyActualIdle2Reloadable")
      ReadyActualIdle2NonReloadable:
        0P00 A 0 A_JumpIfInventory("HalfLifeOpposingForce",1,"ReadyActualIdle2NonReloadableShephard")
        0P00 A 1 A_WeaponReady
        0P03 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady
        0P04 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady
        0P05 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady
        0P06 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady
        0P07 ABCDEFGHIJKLMNOP 1 A_WeaponReady
        goto ReadyActual

      ReadyActualIdle2Reloadable:
        0P00 A 0 A_JumpIfInventory("HalfLifeOpposingForce",1,"ReadyActualIdle2ReloadableShephard")
        0P00 A 1 A_WeaponReady(WRF_ALLOWRELOAD)
        0P03 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady(WRF_ALLOWRELOAD)
        0P04 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady(WRF_ALLOWRELOAD)
        0P05 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady(WRF_ALLOWRELOAD)
        0P06 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady(WRF_ALLOWRELOAD)
        0P07 ABCDEFGHIJKLMNOP 1 A_WeaponReady(WRF_ALLOWRELOAD)
        goto ReadyActual

      ReadyActualAmmoCheck:
        0P00 A 0 A_JumpIfInventory("SamsaraReloadMode",2,1)
        goto ReadyActual
        0P00 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,"ReadyActual")
        0P00 A 0 A_JumpIfInventory("HLSporeCounter",1,"ReadyActual")
        goto ReloadCheck

      Select:
        TNT1 A 0 A_TakeInventory("freeman_fistselected")
        TNT1 A 0 A_TakeInventory("freeman_pistolselected")
        TNT1 A 0 A_TakeInventory("freeman_slot1selected")
        TNT1 A 0 A_TakeInventory("freeman_slot2selected")
        TNT1 A 0 A_TakeInventory("freeman_slot3selected")
        TNT1 A 0 A_TakeInventory("freeman_slot4selected")
        TNT1 A 0 A_GiveInventory("freeman_slot5selected")
        TNT1 A 0 A_TakeInventory("freeman_slot6selected")
        TNT1 A 0 A_TakeInventory("freeman_slot7selected")
        TNT1 A 0 A_TakeInventory("freeman_uniqueselected")
        TNT1 A 0 A_TakeInventory("freeman_unique2selected")
        TNT1 A 0 A_TakeInventory("freeman_unique3selected")
        goto Super::Select

      Ready: // our "Select" state
        TNT1 A 0 A_JumpIfInventory("HalfLifeOpposingForce",1,"ReadyShephard")
        0P15 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady(WRF_NOBOB|WRF_NOFIRE)
        0P16 ABCD 1 A_WeaponReady(WRF_NOBOB|WRF_NOFIRE)
        goto ReadyActual

      Fire:
        0P12 A 0 A_JumpIfInventory("HLOriginalKeySwitcherOpen",1,"ReadyActual")
        0P12 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"FireReloadable")
      FireNonReloadable:
        0P12 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"FireStart")
        0P12 A 0 A_JumpIfInventory("RocketAmmo",1,"FireStart")
        goto FireDry

      FireReloadable:
        0P12 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,"FireStart")
        0P12 A 0 A_JumpIfInventory("HLSporeCounter",1,"FireStart")
        goto FireDry

      FireDry:
        0P00 A 0 A_ClearReFire
        0P00 A 0 A_JumpIfInventory("HalfLifeOpposingForce",1,"FireDryShephard")
        0P00 A 4 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        goto ReadyActualAmmoCheck

      FireStart:
        0P12 A 0 A_AlertMonsters
        0P12 A 0 A_GiveInventory("SamsaraHalfLifeSporeLauncherPrimaryAttackHandler")
        0P12 A 0 A_JumpIfInventory("HalfLifeOpposingForce",1,"FireStartShephard")
        0P12 AB 2 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        0P12 CDEFGHIJKLMO 1 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        goto ReadyActual

      AltFire:
        0P12 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"AltFireReloadable")
      AltFireNonReloadable:
        0P12 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"AltFireStart")
        0P12 A 0 A_JumpIfInventory("RocketAmmo",1,"AltFireStart")
        goto FireDry

      AltFireReloadable:
        0P12 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,"AltFireStart")
        0P12 A 0 A_JumpIfInventory("HLSporeCounter",1,"AltFireStart")
        goto FireDry

      AltFireStart:
        0P12 A 0 A_AlertMonsters
        0P12 A 0 A_GiveInventory("SamsaraHalfLifeSporeLauncherSecondaryAttackHandler")
        0P12 A 0 A_JumpIfInventory("HalfLifeOpposingForce",1,"AltFireStartShephard")
        0P12 AB 2 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        0P12 CDEFGHIJKLMO 1 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        goto ReadyActual

      Reload:
      ReloadCheck:
        0P00 A 0 A_JumpIfInventory("HLSporeCounter",0,"ReadyActual")
        0P00 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,2)
        0P00 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"ReloadStart")
        0P00 A 0 A_JumpIfInventory("RocketAmmo",1,"ReloadStart")
        goto ReadyActual

      ReloadStart:
        0P08 A 0 A_JumpIfInventory("HalfLifeOpposingForce",1,"ReloadStartShephard")
        0P08 ABCDEFGHIJKLMNOPQRST 1 A_WeaponReady(WRF_NOFIRE)
      ReloadLoopInit:
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,1)
        goto ReloadFinish
        // allow interrupting the sequence before ammo calculations take effect by firing either mode
        TNT1 A 0 A_JumpIfInventory("SamsaraSynthFireLeft",1,"ReloadLoopFireCheckPrimary")
        TNT1 A 0 A_JumpIfInventory("SamsaraSynthFireRight",1,"ReloadLoopFireCheckSecondary")
      ReloadLoopAmmoCheck:
        TNT1 A 0 A_JumpIfInventory("HLSporeCounter",0,"ReloadFinish")
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,2)
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"ReloadAmmo")
        TNT1 A 0 A_JumpIfInventory("RocketAmmo",1,"ReloadAmmo")
        goto ReloadFinish

      ReloadLoopFireCheckPrimary:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,"Fire")
        TNT1 A 0 A_JumpIfInventory("HLSporeCounter",1,"Fire")
        goto ReloadLoopAmmoCheck

      ReloadLoopFireCheckSecondary:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,"AltFire")
        TNT1 A 0 A_JumpIfInventory("HLSporeCounter",1,"AltFire")
        goto ReloadLoopAmmoCheck

      ReloadLoopAnim:
        0P09 A 0 A_PlaySound("HLSporeLauncher/Reload")
        0P09 A 0 A_JumpIfInventory("HalfLifeOpposingForce",1,"ReloadLoopAnimShephard")
        0P09 ABCDEFGHIJK 1 A_WeaponReady
        TNT1 A 0
        0P09 LMNOPQRSTUVWYZ 1 A_WeaponReady
        0P10 ABCD 1 A_WeaponReady
        goto ReloadLoopInit

      ReloadFinish:
        0P11 A 0 A_JumpIfInventory("HalfLifeOpposingForce",1,"ReloadFinishShephard")
        0P11 ABCDEFGHIJKLMNOPQRSTUVWXY 1 A_WeaponReady
        goto ReadyActual

      ReloadAmmo: // calculation timing is done in accordance to holding the reload button during the animation in opfor, this is reflected here as well
        0P09 A 0 A_TakeInventory("SamsaraReloadCalcs") // reset
      ReloadAmmoCalc:
        0P09 A 0 A_JumpIfInventory("HLSporeCounter",0,"ReloadLoopAnim")
        0P09 A 0 A_JumpIfInventory("SamsaraReloadCalcs",2,"ReloadLoopAnim")
        0P09 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,2)
        0P09 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,2)
        0P09 A 0 A_JumpIfInventory("RocketAmmo",1,1)
        goto ReloadLoopAnim
        0P09 A 0 A_GiveInventory("SamsaraReloadCalcs")
        0P09 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,2)
        0P09 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,2)
        0P09 A 0 A_TakeInventory("RocketAmmo",1)
        0P09 A 0 A_GiveInventory("HLSporeCounter")
        loop

      // Adrian Shephard
      ReadyActualNonReloadableShephard:
        OP00 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady
        OP01 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady
        OP02 ABCDEFGH 1 A_WeaponReady
        goto ReadyActualJump

      ReadyActualReloadableShephard:
        OP00 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady(WRF_ALLOWRELOAD)
        OP01 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady(WRF_ALLOWRELOAD)
        OP02 ABCDEFGH 1 A_WeaponReady(WRF_ALLOWRELOAD)
        goto ReadyActualJump

      ReadyActualIdle1NonReloadableShephard:
        OP00 A 1 A_WeaponReady
        OP17 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady
        OP18 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady
        OP19 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady
        OP20 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady
        OP21 ABCDEFGHIJKLMNOP 1 A_WeaponReady
        goto ReadyActual

      ReadyActualIdle1ReloadableShephard:
        OP00 A 1 A_WeaponReady(WRF_ALLOWRELOAD)
        OP17 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady(WRF_ALLOWRELOAD)
        OP18 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady(WRF_ALLOWRELOAD)
        OP19 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady(WRF_ALLOWRELOAD)
        OP20 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady(WRF_ALLOWRELOAD)
        OP21 ABCDEFGHIJKLMNOP 1 A_WeaponReady(WRF_ALLOWRELOAD)
        goto ReadyActual

      ReadyActualIdle2NonReloadableShephard:
        OP00 A 1 A_WeaponReady
        OP03 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady
        OP04 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady
        OP05 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady
        OP06 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady
        OP07 ABCDEFGHIJKLMNOP 1 A_WeaponReady
        goto ReadyActual

      ReadyActualIdle2ReloadableShephard:
        OP00 A 1 A_WeaponReady(WRF_ALLOWRELOAD)
        OP03 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady(WRF_ALLOWRELOAD)
        OP04 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady(WRF_ALLOWRELOAD)
        OP05 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady(WRF_ALLOWRELOAD)
        OP06 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady(WRF_ALLOWRELOAD)
        OP07 ABCDEFGHIJKLMNOP 1 A_WeaponReady(WRF_ALLOWRELOAD)
        goto ReadyActual

      ReadyShephard:
        OP15 ABCDEFGHIJKLMNOPQRSTUVWXYZ 1 A_WeaponReady(WRF_NOBOB|WRF_NOFIRE)
        OP16 ABCD 1 A_WeaponReady(WRF_NOBOB|WRF_NOFIRE)
        goto ReadyActual

      FireDryShephard:
        OP00 A 4 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        goto ReadyActualAmmoCheck

      FireStartShephard:
        OP12 AB 2 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        OP12 CDEFGHIJKLMO 1 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        goto ReadyActual

      AltFireStartShephard:
        OP12 AB 2 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        OP12 CDEFGHIJKLMO 1 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        goto ReadyActual

      ReloadStartShephard:
        OP08 ABCDEFGHIJKLMNOPQRST 1 A_WeaponReady(WRF_NOFIRE)
        goto ReloadLoopInit

      ReloadLoopAnimShephard:
        OP09 ABCDEFGHIJK 1 A_WeaponReady
        TNT1 A 0
        OP09 LMNOPQRSTUVWYZ 1 A_WeaponReady
        OP10 ABCD 1 A_WeaponReady
        goto ReloadLoopInit

      ReloadFinishShephard:
        OP11 ABCDEFGHIJKLMNOPQRSTUVWXY 1 A_WeaponReady
        goto ReadyActual
    }
}

actor SamsaraHalfLifeSporeLauncherPrimaryAttackHandler : Trigger
{
    States
    {
      // Ammo checking/taking stuff
      Pickup:
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"PickupReloadable")
      PickupNonReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"PickupAttack")
        TNT1 A 0 A_JumpIfInventory("RocketAmmo",1,1)
        stop
        TNT1 A 0 A_TakeInventory("RocketAmmo",1)
        goto PickupAttack

      PickupReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,"PickupAttack")
        TNT1 A 0 A_JumpIfInventory("HLSporeCounter",1,1)
        stop
        TNT1 A 0 A_TakeInventory("HLSporeCounter",1)
        goto PickupAttack

      // Attack stuff
      PickupAttack:
        TNT1 A 0 A_PlaySound("HLSporeLauncher/Fire",CHAN_WEAPON)
        TNT1 A 0 A_JumpIfInventory("SPModeOn",1,"PickupAttackCoop")
        TNT1 A 0 A_JumpIfInventory("CoopModeOn",1,"PickupAttackCoop")
      PickupAttackDM:
        TNT1 A 0 A_FireCustomMissile("HLSporeLauncherSporeMissile",0,false,8)
        goto PickupFinish

      PickupAttackCoop:
        TNT1 A 0 A_FireCustomMissile("HLSporeLauncherSporeMissileCoop",0,false,8)
        goto PickupFinish

      // Finishing stuff
      PickupFinish:
        TNT1 A 0 ACS_NamedExecuteAlways("HalfLifeRecoil", 0, 2)
        stop
    }
}

actor SamsaraHalfLifeSporeLauncherSecondaryAttackHandler : Trigger
{
    States
    {
      // Ammo checking/taking stuff
      Pickup:
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"PickupReloadable")
      PickupNonReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"PickupAttack")
        TNT1 A 0 A_JumpIfInventory("RocketAmmo",1,1)
        stop
        TNT1 A 0 A_TakeInventory("RocketAmmo",1)
        goto PickupAttack

      PickupReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,"PickupAttack")
        TNT1 A 0 A_JumpIfInventory("HLSporeCounter",1,1)
        stop
        TNT1 A 0 A_TakeInventory("HLSporeCounter",1)
        goto PickupAttack

      // Attack stuff
      PickupAttack:
        TNT1 A 0 A_PlaySound("HLSporeLauncher/AltFire",CHAN_WEAPON)
        TNT1 A 0 A_JumpIfInventory("SPModeOn",1,"PickupAttackCoop")
        TNT1 A 0 A_JumpIfInventory("CoopModeOn",1,"PickupAttackCoop")
      PickupAttackDM:
        TNT1 A 0 A_FireCustomMissile("HLSporeLauncherSporeGrenade",0,false,8)
        goto PickupFinish

      PickupAttackCoop:
        TNT1 A 0 A_FireCustomMissile("HLSporeLauncherSporeGrenadeCoop",0,false,8)
        goto PickupFinish

      // Finishing stuff
      PickupFinish:
        TNT1 A 0 ACS_NamedExecuteAlways("HalfLifeRecoil", 0, 2)
        stop
    }
}

actor HLSporeCounter : Ammo
{
    Inventory.MaxAmount 10
    Tag "Spore Launcher Magazine"
    +IGNORESKILL
}

Actor HLSporeLauncherSporeMissile
{
    Renderstyle add
    Alpha 0.85
    Radius 8
    Height 16
    Speed 20
    Damage (75)
    PROJECTILE
    +NOGRAVITY
    +EXTREMEDEATH
    Scale 0.9
    DamageType "HLRPGExplosive"
    PoisonDamage 1, 3, 1
    Decal "SporeSplat"
    States
    {
        Spawn:
            TNT1 A 0
        Spawn2:
            TNT1 A 0 A_SpawnItemEx("HLSporeFlare",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION)
            TNT1 A 0 A_SpawnItemEx("HLSporeLauncherSporeGrenadeTrail",0,0,0,0,FRandom(-2,2),0,0,SXF_NOCHECKPOSITION)
            HL00 A 1 Bright
            Loop
        Death:
            TNT1 A 0 A_PlaySound("HLSporeLauncher/Impact", CHAN_BODY, 1.0)
            TNT1 A 0 A_Explode(150,104)
            TNT1 A 5 A_SpawnItemEx("HLSporeExplode",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION)
            Stop
    }
}

