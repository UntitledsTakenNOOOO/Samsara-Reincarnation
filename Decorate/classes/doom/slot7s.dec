actor DGHasBFG10K : Boolean {}
actor SamsaraDoomguyStrDevastatorHeldCheck : Boolean {}

ACTOR " BFG10K " : Weapon
{
	Radius 20
	Height 20
	Weapon.Selectionorder 2800
	Weapon.AmmoUse 5
	Weapon.AmmoGive 0
	Weapon.AmmoType "Cell"
	Weapon.Kickback 100
	Inventory.Pickupmessage "You got the BFG10000! Hell, yes!"
	Inventory.RestrictedTo "DoomguyPlayer"
	Obituary "$SAMSARA_DOOMGUY_OB_SLOT7S"
	+AMMO_OPTIONAL
	+BFG
	+EXPLOSIVE
	+NOALERT
	+NOAUTOFIRE
	+NOLMS
	+UNDROPPABLE
	Tag "B.F.G. 10000"
	States
	{
	Spawn:
		WBG2 A -1
		stop

	// Original
	Ready:
		TNT1 A 0 A_JumpIfInventory("DoomClassMode",2,"ReadyStr")
		TNT1 A 0 A_JumpIfInventory("DoomClassMode",1,"Ready64")
		TNT1 A 0 A_GiveInventory("SamsaraDoomguyBFG10KReadySoundPlayer")
		DBG2 AAABBBCCCDDD 1 Bright A_WeaponReady
		loop

	Deselect:
		TNT1 A 0 A_JumpIfInventory("DoomClassMode",2,"DeselectStr")
		TNT1 A 0 A_JumpIfInventory("DoomClassMode",1,"Deselect64")
		DBG2 E 1 Bright A_Lower
		loop

	Select:
		TNT1 A 0 A_JumpIfInventory("DoomClassMode",2,"SelectStr")
		TNT1 A 0 A_JumpIfInventory("DoomClassMode",1,"Select64")
		DBG2 E 1 Bright A_Raise
		loop

	Fire:
		TNT1 A 0 A_JumpIfInventory("DoomClassMode",2,"FireStr")
		TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"FireStart")
		TNT1 A 0 A_JumpIfInventory("Cell",5,"FireStart")
		goto Nothing

	FireStart:
		TNT1 A 0 A_AlertMonsters
		TNT1 A 0 A_JumpIfInventory("DoomClassMode",1,"Fire64")
		DBG2 E 20 Bright A_GiveInventory("SamsaraDoomguyBFG10KPreFireSoundPlayer")
		DBG2 F 4 Bright
		DBG2 GHIJ 1 Bright
		goto HoldStart

	Hold:
		TNT1 A 0 A_JumpIfInventory("DoomClassMode",2,"HoldStr")
		TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,2)
		TNT1 A 0 A_JumpIfInventory("Cell",5,1)
		goto Nothing
		TNT1 A 0 A_AlertMonsters
		TNT1 A 0 A_JumpIfInventory("DoomClassMode",1,"Hold64")
	HoldStart:
		DBG2 K 2 Bright A_GunFlash
		DBG2 L 2 Bright A_GiveInventory("SamsaraDoomguyBFG10KAttackHandler")
		DBG2 M 2 Bright 
		TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,2)
		TNT1 A 0 A_JumpIfInventory("Cell",5,1)
		goto Nothing
		TNT1 A 0 A_ReFire
		DBG2 O 35 A_GiveInventory("SamsaraDoomguyBFG10KFireFinishSoundPlayer")
		goto Ready

	Flash:
		TNT1 A 2 Bright A_Light1
		TNT1 A 3 Bright
		goto LightDone

	// Doom 64 (has kickback, has blend effect on puff)
	Ready64:
		TNT1 A 0 A_PlaySound("doom64guy/bfg10kidle")
		64BG AAABBBCCC 1 Bright A_WeaponReady
		goto Ready

	Deselect64:
		64BG A 1 Bright A_Lower
		goto Deselect

	Select64:
		64BG A 1 Bright A_Raise
		goto Select

	Fire64:
		64BG D 20 Bright A_PlaySound("doom64guy/bfg10kf")
		64BG E 4 Bright 
		64BG FGHI 1 Bright
	Hold64:
		64BG J 2 Bright A_GunFlash
        TNT1 A 0 A_GiveInventory("SamsaraDoomguy64BFG10KAttackHandler")
		64BG K 1 Bright A_JumpIfInventory("SamsaraHasDoubleFiringSpeed",1,1)
		64BG L 1 Bright
		64BG M 1 Bright A_JumpIfInventory("SamsaraHasDoubleFiringSpeed",1,1)
		64BG N 1 Bright
		TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,2)
		TNT1 A 0 A_JumpIfInventory("Cell",5,1)
		goto Nothing
		TNT1 A 0 A_ReFire
		64BG O 35 A_PlaySound("doom64guy/bfg10kcool")
		goto Ready

	// Stronghold (Devastator)
	ReadyStr:
		DEVG A 1 A_WeaponReady
		goto Ready

	// Unpowered
	DeselectStr:
		TNT1 A 0 A_JumpIfInventory("PowerSamsaraDoomguyStrWeaponPower",1,"DeselectStrPowered")
		TNT1 A 0 A_JumpIfInventory("SamsaraHasPermaWeaponLevel2",1,"DeselectStrPowered")
	DeselectStrContinue:
		TNT1 A 0 A_Lower
		DEVG A 1 A_Lower
		goto Deselect

	SelectStr:
		TNT1 A 0 A_JumpIfInventory("PowerSamsaraDoomguyStrWeaponPower",1,"SelectStrPowered")
		TNT1 A 0 A_JumpIfInventory("SamsaraHasPermaWeaponLevel2",1,"SelectStrPowered")
	SelectStrContinue:
		TNT1 A 0 A_Raise
		DEVG A 1 A_Raise
		goto Select

	FireStr:
		TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,2)
		TNT1 A 0 A_JumpIfInventory("Cell",60,1)
		goto Nothing
		TNT1 A 0 A_AlertMonsters
		TNT1 A 0 A_TakeInventory("SamsaraDoomguyStrDevastatorHeldCheck")
		TNT1 A 0 A_PlaySound("doomguy/stronghold/devchr",CHAN_WEAPON)
		DEVG BCDEF 10 Bright
	FireStrContinue:
		TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,2)
		TNT1 A 0 A_JumpIfInventory("Cell",60,1)
		goto Nothing
		TNT1 A 0 A_ReFire // skipped entirely when preparing a weapon switch, we don't want that
		TNT1 A 0 A_JumpIfInventory("SamsaraSynthFireLeft",1,"Hold") // "weapon switch delay" hack
		DEVG J 5 Bright A_GunFlash("FlashStr")
		DEVG K 10 Bright A_GiveInventory("SamsaraDoomguyStrDevastatorRangedAttackHandler")
		DEVG L 7 Bright
		goto Ready

	HoldStr:
		TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,2)
		TNT1 A 0 A_JumpIfInventory("Cell",60,1)
		goto Nothing
		TNT1 A 0 A_AlertMonsters
		TNT1 A 0 A_GiveInventory("SamsaraDoomguyStrDevastatorHeldCheck")
		TNT1 A 0 A_PlaySound("doomguy/stronghold/devfch",CHAN_WEAPON)
		DEVG GHIGHI 4 Bright A_GiveInventory("SamsaraDoomguyStrDevastatorMeleeAttackHandler")
		goto FireStrContinue

	FlashStr:
		TNT1 A 5 A_Light1
		TNT1 A 10 A_Light2
		TNT1 A 7 A_Light1
		goto LightDone

	// Powered
	DeselectStrPowered:
		TNT1 A 0 A_Lower
		goto DeselectStrContinue

	SelectStrPowered:
		TNT1 A 0 A_Raise
		goto SelectStrContinue

	Nothing:
        TNT1 A 0 A_ClearReFire
        TNT1 A 0 A_JumpIfInventory("DoomClassMode",2,"NothingStr")
        TNT1 A 0 A_JumpIfInventory("DoomClassMode",1,"Nothing64")
        TNT1 A 0 A_JumpIfInventory("SamsaraDoomOriginalLoadoutFallbackRanged",1,2)
        TNT1 A 0 A_SelectWeapon(" Pistol ")
        goto Deselect
        TNT1 A 0 A_SelectWeapon(" Rifle ")
        goto Deselect

	Nothing64:
	NothingStr:
        TNT1 A 0 A_SelectWeapon(" Pistol ")
        goto Deselect
	}
}

actor SamsaraDoomguyBFG10KAttackHandler : Trigger
{
    States
    {
      // Ammo checking/taking stuff
      Pickup:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"PickupAttack")
        TNT1 A 0 A_JumpIfInventory("Cell",5,1)
        stop
        TNT1 A 0 A_TakeInventory("Cell",5)
        goto PickupAttack

      // Attack stuff
      PickupAttack:
        TNT1 A 0 A_JumpIfInventory("SamsaraAlternateSoundToken",2,"PickupAttackPerkristan")
        TNT1 A 0 A_JumpIfInventory("SamsaraAlternateSoundToken",1,"PickupAttackPSX")
        // Doom (Original) sounds
      PickupAttackOriginal:
        TNT1 A 0 A_PlaySound("doomguy/bfg10kfire",CHAN_WEAPON)
        TNT1 A 0 A_JumpIfInventory("SPModeOn",1,"PickupAttackOriginalCoop")
        TNT1 A 0 A_JumpIfInventory("CoopModeOn",1,"PickupAttackOriginalCoop")
      PickupAttackOriginalDM:
        TNT1 A 0 A_FireBullets(0,0,1,0,"ST_BFG10kShot",FBF_NOFLASH)
        stop

      PickupAttackOriginalCoop:
        TNT1 A 0 A_FireBullets(0,0,1,0,"ST_BFG10kShot_Coop",FBF_NOFLASH)
        stop

        // Doom (PSX) sounds
      PickupAttackPSX:
        TNT1 A 0 A_PlaySound("doom64guy/bfg10kfire",CHAN_WEAPON)
        TNT1 A 0 A_JumpIfInventory("SPModeOn",1,"PickupAttackPSXCoop")
        TNT1 A 0 A_JumpIfInventory("CoopModeOn",1,"PickupAttackPSXCoop")
      PickupAttackPSXDM:
        TNT1 A 0 A_FireBullets(0,0,1,0,"ST_BFG10kShotSoundPSX",FBF_NOFLASH)
        stop

      PickupAttackPSXCoop:
        TNT1 A 0 A_FireBullets(0,0,1,0,"ST_BFG10kShot_CoopSoundPSX",FBF_NOFLASH)
        stop

        // Perkristian Hi-Res Sound Pack sounds
      PickupAttackPerkristan:
        TNT1 A 0 A_PlaySound("doomguy/perkristan/bfg10kfire",CHAN_WEAPON)
        TNT1 A 0 A_JumpIfInventory("SPModeOn",1,"PickupAttackPerkristanCoop")
        TNT1 A 0 A_JumpIfInventory("CoopModeOn",1,"PickupAttackPerkristanCoop")
      PickupAttackPerkristanDM:
        TNT1 A 0 A_FireBullets(0,0,1,0,"ST_BFG10kShotSoundPerkristan",FBF_NOFLASH)
        stop

      PickupAttackPerkristanCoop:
        TNT1 A 0 A_FireBullets(0,0,1,0,"ST_BFG10kShot_CoopSoundPerkristan",FBF_NOFLASH)
        stop
    }
}

