	MAU2 A 0 A_JumpIfInventory("Cell",7,"AltFire2")
	MAU2 A 0 A_JumpIfInventory("Cell",6,"AltFire2")
	MAU2 A 0 A_JumpIfInventory("Cell",5,"AltFire2")
	MAU2 A 0 A_JumpIfInventory("Cell",4,"AltFire2")
	MAU2 A 0 A_JumpIfInventory("Cell",3,"AltFire2")
	MAU2 A 0 A_JumpIfInventory("Cell",2,"AltFire2")
	MAU2 A 0 A_JumpIfInventory("Cell",1,"AltFire2")
    MAU2 A 0 A_JumpIfInventory("CoopModeOn", 1, "CoopFire2")
    MAU2 A 20 A_PlayWeaponSound("strife/maulercharge")
    MAU2 B 10 A_Light1
	MAU2 B 0 Bright A_Recoil(8)
	MAUL A 0 Bright A_TakeInventory("Cell",20)
    MAUL A 10 Bright A_FireCustomMissile("StrifeMaulerTorpedo",0,1,0,8,0)
//	MAUL A 0 Bright DamageThing(20)
    MAUL B 10 Bright A_Light2
    MAUL C 2
    MAUL D 2 A_Light0
    MAUL E 2 A_ReFire
    Goto Ready
  CoopFire2:
    MAU2 A 20 A_PlayWeaponSound("strife/maulercharge")
    MAU2 B 10 A_Light1
	MAU2 B 0 Bright A_Recoil(8)
	MAUL A 0 Bright A_TakeInventory("Cell",20)
    MAUL A 10 Bright A_FireCustomMissile("StrifeMaulerTorpedo2",0,1,0,8,0)
//	MAUL A 0 Bright DamageThing(20)
    MAUL B 10 Bright A_Light2
    MAUL C 2
    MAUL D 2 A_Light0
    MAUL E 2 A_ReFire
    Goto Ready
  }
}

actor StrifeMaulerPuff : MaulerPuff replaces MaulerPuff
{
  +NOBLOCKMAP
  +NOGRAVITY
  +PUFFONACTORS
  +FORCEXYBILLBOARD  
  RenderStyle Add
  DamageType Disintegrate
  Decal "RailScorchLower"
  DamageType "Disintegrate"
  States
  {
  Spawn:
    MPUF AB 5
    SPW1 ABCDE 4
    Stop
  }
}

actor StrifeMaulerBullet : FastProjectile
{
    //Damage (Random(2,3)*random(10,15)) // 20 - 45
	Damage (Random(1,3) * 10)
    Radius 1
    Height 1
    Speed 320
    Projectile
    Renderstyle "Normal"
    Species "Player"
	DamageType "Disintegrate"
	+BLOODSPLATTER
    +THRUSPECIES
    +NOTIMEFREEZE
    Alpha 1.0
    Decal Bulletchip
	
    States
    {
      Spawn:
        TNT1 A 1
        loop
	  Crash:
      Death:
        TNT1 A 1 A_SpawnItem("StrifeMaulerPuff")
        stop
      XDeath:
        TNT1 A 1 A_SpawnItem("StrifeMaulerPuff")
        stop
    }
}

actor StrifeMaulerTorpedo : MaulerTorpedo replaces MaulerTorpedo
{
  Damage (Random(1,4)*1)
  SeeSound "strife/maulerfire2"
  DeathSound "strife/maulerhit"
  Obituary "%k \civiciously vaporized\c- %o\ci."
  DamageType "Disintegrate"
  +FORCEXYBILLBOARD
  States
  {
  Spawn:
    MTRP ABCD 4 Bright
    Loop
  Death:
    MTHT AB 8 Bright
    MTHT CC 0 Bright A_MaulerTorpedoWave
	MTHT C 8 Bright
    MTHT DE 8 Bright
    Stop
  }
}

