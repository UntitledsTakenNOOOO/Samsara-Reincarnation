actor SW_RabbitMale : SamsaraMiscCharAnimalStaticTotalGib
{
	Monster
	-COUNTKILL
	+FRIENDLY
	+NOBLOODDECALS
	+THRUSPECIES
	+NEVERTARGET
	Height 24
	Radius 12
	Health 1
	Mass 15
	Scale 0.25
	Gravity 0.33
	Tag "Rabbit"
	Species "SWRabbit"
	BloodType "SWDefaultBloodSpawner"
	DeathSound "sw/rabbit/die"
	Speed 6
	PainChance "ShrinkerDMG", 0
	States
	{
		Spawn:
			SW65 A 1
			SW65 A 0 ACS_NamedExecuteWithResult("SW_RabbitGrowUp")
			Goto Roam
		Screw:
			SW65 A 0 A_ChangeFlag("PUSHABLE",0)
			SW65 A 0 A_ChangeFlag("INVULNERABLE",1)
			SW65 A 0 A_SetMass(999999)
			SW65 ACACACACACACACACACACACACACAC 6
			Goto FinishScrew
		FinishScrew:
			SW65 A 0 A_TakeInventory("SWRabbitScrewing",1)
			SW65 A 0 A_SetMass(15)
			SW65 A 0 A_ChangeFlag("INVULNERABLE",0)
			SW65 A 0 A_ChangeFlag("SHOOTABLE",1)
			Goto Roam
		Roam:
			SW65 A 0 A_Jump(16,"Stand")
			SW65 A 0 A_Jump(8,"Leap")
			SW65 A 2 A_Wander
			SW65 B 0 A_Jump(16,"Stand")
			SW65 B 0 A_Jump(8,"Leap")
			SW65 B 2 A_Wander
			SW65 C 0 A_Jump(16,"Stand")
			SW65 C 0 A_Jump(8,"Leap")
			SW65 C 2 A_Wander
			SW65 D 0 A_Jump(16,"Stand")
			SW65 D 0 A_Jump(8,"Leap")
			SW65 D 2 A_Wander
			SW65 E 0 A_Jump(16,"Stand")
			SW65 E 0 A_Jump(8,"Leap")
			SW65 E 2 A_Wander
			Loop
		Stand:
			SW65 AE 3 A_Jump(20,"Roam")
			Loop
		Leap:
			SW65 A 0 ThrustThingZ(0,20,0,0)
			SW65 A 0 A_Recoil(-4)
			SW65 AB 4
		Leaping:
			SW65 B 4 A_CheckFloor("Roam")
			Loop
		Death:
			TNT1 A 0 A_SpawnItem("UniversalBloodyHellGoreDeathEffect")
			TNT1 A 0 A_Scream
			TNT1 A 0 A_Fall
			TNT1 A 0 A_SpawnItemEx("SWBunnyHeadGibMale",0,0,0,frandom(-1.0,1.0),frandom(-1.0,1.0),frandom(0.5,1.0),0,SXF_TRANSFERSCALE)		
			TNT1 A 5 A_FadeOut
			Stop
		XDeathForWang:
			TNT1 A 0 A_Jump(218, 2)
			TNT1 A 0 A_GiveToTarget("LoWangDMTaunt2",1)
			TNT1 A -1 A_Jump(256,"Death")
			wait
	}
}

