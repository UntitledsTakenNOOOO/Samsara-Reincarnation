actor KamikazeImmunity: PowerProtection
{
    DamageFactor "Kamikaze", 0
    DamageFactor "ZorchKamikaze", 0

    Powerup.Duration 4
}

actor PeopleDieParticle
{
    +NOINTERACTION
    +CLIENTSIDEONLY
    
    Radius 0
    Height 0

    States
    {
      Spawn:
        TNT1 A 70
        stop
    }
}

