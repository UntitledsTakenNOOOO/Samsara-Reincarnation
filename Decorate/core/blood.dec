actor SamsaraBloodyHellBlood : Blood replaces Blood
{
	Game Doom
	+PUFFGETSOWNER
	States
	{
	Spawn:
		TNT1 A 0
		Goto Spawn+4
		TNT1 A 0
		Goto Spawn+5
		TNT1 A 0
		TNT1 A 3 ACS_NamedExecuteWithResult("SamsaraClientDecorate",12, 2)
		Stop
		TNT1 A 3 ACS_NamedExecuteWithResult("SamsaraClientDecorate",12, 0)
		Stop
		TNT1 A 3 ACS_NamedExecuteWithResult("SamsaraClientDecorate",12, 1)
		Stop
	Spray:
		TNT1 A 0
		TNT1 A 3 ACS_NamedExecuteWithResult("SamsaraClientDecorate",12, 3)
		Stop
	SpawnHappyFun:
	SpawnNightmare:
	SpawnNashGore:
	SpawnNashGore2D:
	SpawnBrutal2D:
	SpawnNormal:
		BLUD C 8 A_SpawnItemEx("Switchable_FlyingBlood",0,0,0,random(-350,350)/100.00,random(-350,350)/100.00,random(500,1000)/100.00,random(-180,180),SXF_TRANSFERTRANSLATION|SXF_NOCHECKPOSITION)
		BLUD BA 8
		Stop
	SpawnHappyFunMedium:
	SpawnNightmareMedium:
	SpawnNashGoreMedium:
	SpawnNashGore2DMedium:
	SpawnBrutal2DMedium:
	SpawnNormalMedium:
		BLUD B 8 A_SpawnItemEx("Switchable_FlyingBlood",0,0,0,random(-350,350)/100.00,random(-350,350)/100.00,random(500,1000)/100.00,random(-180,180),SXF_TRANSFERTRANSLATION|SXF_NOCHECKPOSITION)
		BLUD A 8
		Stop
	SpawnHappyFunSmall:
	SpawnNightmareSmall:
	SpawnNashGoreSmall:
	SpawnNashGore2DSmall:
	SpawnBrutal2DSmall:
	SpawnNormalSmall:
		BLUD A 8 A_SpawnItemEx("Switchable_FlyingBlood",0,0,0,random(-350,350)/100.00,random(-350,350)/100.00,random(500,1000)/100.00,random(-180,180),SXF_TRANSFERTRANSLATION|SXF_NOCHECKPOSITION)
		Stop
	SpawnHappyFunSpray:
	SpawnNightmareSpray:
	SpawnNashGoreSpray:
	SpawnNashGore2DSpray:
	SpawnBrutal2DSpray:
	SpawnNormalSpray:
		SPRY A 3 A_SpawnItemEx("Switchable_FlyingBlood",0,0,0,random(-350,350)/100.00,random(-350,350)/100.00,random(500,1000)/100.00,random(-180,180),SXF_TRANSFERTRANSLATION|SXF_NOCHECKPOSITION)
		SPRY BCDEF 3
		SPRY G 2
		Stop
	SpawnBrutal:
		TNT1 A 0 A_SpawnItemEx("Switchable_FlyingBlood",0,0,0,random(-350,350)/100.00,random(-350,350)/100.00,random(500,1000)/100.00,random(-180,180),SXF_TRANSFERTRANSLATION|SXF_NOCHECKPOSITION)
		TNT1 A 0 A_SetScale(0.33)
		BH2A ABDEFGH 1
		Stop
	SpawnBolognese:
		TNT1 A 0 A_SpawnItemEx("Switchable_FlyingBlood",0,0,0,random(-350,350)/100.00,random(-350,350)/100.00,random(500,1000)/100.00,random(-180,180),SXF_TRANSFERTRANSLATION|SXF_NOCHECKPOSITION)
		TNT1 A 0 A_SetScale(0.8)
		TNT1 A 0 A_SpawnItemEx("Bolognese_OnlineSound1",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION|SXF_CLIENTSIDE)
		TNT1 A 0 A_ChangeFlag("NOCLIP",true)
		TNT1 A 0 A_ChangeFlag("NOGRAVITY",true)
		BSPX A 3 
		BSPX BCDEFGHIJ 1
		TNT1 A 0 A_ChangeFlag("NOGRAVITY",false)
		TNT1 A 0 A_ChangeFlag("NOCLIP",false)
		Stop
	SpawnComic:
		TNT1 A 0 A_CheckFlag("CORPSE","Remove",AAPTR_Target)
		TNT1 A 0 A_JumpIfInTargetInventory("SamsaraComicEffectLimiter",1,4)
		TNT1 A 0 A_GiveToTarget("SamsaraComicEffectLimiter",1)
		TNT1 A 3 A_SpawnItemEx("SamsaraComicEffect",0,0,0,frandom(-1.0,1.0),frandom(-1.0,1.0),frandom(0.5,1.5))
		TNT1 A 0 A_TakeFromTarget("SamsaraComicEffectLimiter",1)
		Goto Remove
	Remove:
		TNT1 A 1
		Stop
	}
}

