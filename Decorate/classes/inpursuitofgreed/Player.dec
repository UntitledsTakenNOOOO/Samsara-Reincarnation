actor IPOGPlayerPainSoundTrigger : Trigger
{
    States
    {
      Pickup:
        TNT1 A 0 A_JumpIfInventory("IPOGMode",4,"PickupDominatrix")
        TNT1 A 0 A_JumpIfInventory("IPOGMode",3,"PickupSpecimen")
        TNT1 A 0 A_JumpIfInventory("IPOGMode",2,"PickupLizard")
        TNT1 A 0 A_JumpIfInventory("IPOGMode",1,"PickupCyborg")
      PickupMooman:
        TNT1 A 0 A_Pain
      PickupFinish:
        TNT1 A 0 A_RailWait
        stop

      PickupCyborg:
        TNT1 A 0 A_PlaySound("G_Cyborg/Pain",CHAN_VOICE)
        goto PickupFinish

      PickupLizard:
        TNT1 A 0 A_PlaySound("G_Lizard/Pain",CHAN_VOICE)
        goto PickupFinish

      PickupSpecimen:
        TNT1 A 0 A_PlaySound("G_Specimen/Pain",CHAN_VOICE)
        goto PickupFinish

      PickupDominatrix:
        TNT1 A 0 A_PlaySound("G_Dominatrix/Pain",CHAN_VOICE)
        goto PickupFinish
    }
}

