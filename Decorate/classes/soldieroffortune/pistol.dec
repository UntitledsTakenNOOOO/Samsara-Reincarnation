			"####" "#" 0 A_JumpIfInventory("SoF_Pistol1Magazine",1,1)
			Goto DryFire
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",11,"FIRE1_C")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",10,"FIRE1_B")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",9,"FIRE1_A")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",8,"FIRE1_C")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",7,"FIRE1_B")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",6,"FIRE1_C")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",5,"FIRE1_B")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",4,"FIRE1_A")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",3,"FIRE1_C")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",2,"FIRE1_B")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",1,"FIRE1_A")
			Goto ReadyLoop
		FIRE1_A:
			"####" "#" 0 A_JumpIfInventory("SoF_FullyLeaned",1,3)
			"####" "#" 0 A_JumpIfInventory("SoF_Leaning",2,2)
			"####" "#" 0 A_JumpIfInventory("SoF_Leaning",1,"IDLE_A_TO_IDLE_B")
			SF00 A 0 A_GunFlash("FLASH1_A")
			SF00 A 0 A_GiveInventory("SoF_Pistol1_A_FireToken")
			SF00 A 1 A_WeaponReady(WRF_NOFIRE|WRF_NOSWITCH)
			SF00 B 2 A_WeaponReady(WRF_NOFIRE|WRF_NOSWITCH)
			SF00 C 1 A_WeaponReady(WRF_NOFIRE|WRF_NOSWITCH)
			SF00 D 2 A_WeaponReady(WRF_NOFIRE|WRF_NOSWITCH)
			SF00 E 1 A_WeaponReady(WRF_NOFIRE|WRF_NOSWITCH)
			SF00 F 2 A_WeaponReady(WRF_NOFIRE|WRF_NOSWITCH)
			SF00 G 1 A_WeaponReady(WRF_NOFIRE|WRF_NOSWITCH)
			SF0A A 0 ACS_NamedExecuteWithResult("SoF_Decorate",1,1)
			Goto ReadyLoop
		FIRE1_B:
			"####" "#" 0 A_JumpIfInventory("SoF_FullyLeaned",1,2)
			"####" "#" 0 A_JumpIfInventory("SoF_Leaning",2,"IDLE_B_TO_IDLE_A")
			SF01 A 0 A_GunFlash("FLASH1_B")
			SF01 A 0 A_GiveInventory("SoF_Pistol1_B_FireToken")
			SF01 A 1 A_WeaponReady(WRF_NOFIRE|WRF_NOSWITCH)
			SF01 B 2 A_WeaponReady(WRF_NOFIRE|WRF_NOSWITCH)
			SF01 C 1 A_WeaponReady(WRF_NOFIRE|WRF_NOSWITCH)
			SF01 D 2 A_WeaponReady(WRF_NOFIRE|WRF_NOSWITCH)
			SF01 E 1 A_WeaponReady(WRF_NOFIRE|WRF_NOSWITCH)
			SF01 F 2 A_WeaponReady(WRF_NOFIRE|WRF_NOSWITCH)
			SF01 G 1 A_WeaponReady(WRF_NOFIRE|WRF_NOSWITCH)
			SF0B A 0 ACS_NamedExecuteWithResult("SoF_Decorate",1,2)
			Goto ReadyLoop
		FIRE1_C:
			"####" "#" 6
			SF01 A 0 ACS_NamedExecuteWithResult("SoF_Decorate",1,2)
			Goto FIRE1_B
		FLASH1_A:
			SF90 A 1 Bright
			SF90 B 0
			Stop
		FLASH1_B:
			SF91 A 1 Bright
			SF91 B 0
			Stop
			
		AltFire:
			"####" "#" 0 A_JumpIfInventory("SoF_FullyLeaned",1,"ReadyLoop")
			"####" "#" 0 A_JumpIfInventory("SoF_SwitchHand",1,"ReadyLoop")
			"####" "#" 0 A_GiveInventory("SoF_SwitchHand",1)
			Goto ReadyLoop
			
		Reload:
			"####" "#" 0 A_JumpIfInventory("SoF_Pistol1Magazine",0,"ReadyLoop")
			"####" "#" 0 A_JumpIfInventory("Clip",1,1)
			Goto ReadyLoop
			"####" "#" 0 A_TakeInventory("SoF_WeaponAnimFrame")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",11,"RELOAD_B")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",10,"RELOAD_B")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",9,"RELOAD_A")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",8,"RELOAD_B")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",7,"RELOAD_B")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",6,"RELOAD_B")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",5,"RELOAD_B")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",4,"RELOAD_A")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",3,"RELOAD_B")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",2,"RELOAD_B")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",1,"RELOAD_A")
		RELOAD_A:
			TNT1 A 0 A_JumpIfInventory("Clip",1,1)
			Goto RELOAD_A_FINISH
			TNT1 A 0 A_GiveInventory("SoF_Pistol1Magazine",1)
			TNT1 A 0 A_TakeInventory("Clip",1,TIF_NOTAKEINFINITE)
			TNT1 A 0 A_JumpIfInventory("SoF_Pistol1Magazine",0,"RELOAD_A_FINISH")
			Loop
		RELOAD_A_FINISH:
			SF02 A 0 A_PlaySound("SoF/Weapons/gpistol/ReloadIn",CHAN_AUTO)
			SF02 ABCDEFG 2 A_WeaponReady(WRF_NOFIRE|WRF_NOSWITCH)
			SF02 H 0 A_PlaySound("SoF/Weapons/gpistol/ReloadOut",CHAN_AUTO)
			SF02 HIJKL 2 A_WeaponReady(WRF_NOFIRE|WRF_NOSWITCH)
			SF02 M 0 A_PlaySound("SoF/Weapons/gpistol/ReloadFIn",CHAN_AUTO)
			SF02 MN 2 A_WeaponReady(WRF_NOFIRE|WRF_NOSWITCH)
			SF0A A 0 ACS_NamedExecuteWithResult("SoF_Decorate",1,1)
			Goto ReadyLoop
		RELOAD_B:
			TNT1 A 0 A_JumpIfInventory("Clip",1,1)
			Goto RELOAD_B_FINISH
			TNT1 A 0 A_GiveInventory("SoF_Pistol1Magazine",1)
			TNT1 A 0 A_TakeInventory("Clip",1,TIF_NOTAKEINFINITE)
			TNT1 A 0 A_JumpIfInventory("SoF_Pistol1Magazine",0,"RELOAD_B_FINISH")
			Loop
		RELOAD_B_FINISH:
			SF03 A 0 A_PlaySound("SoF/Weapons/gpistol/ReloadIn",CHAN_AUTO)
			SF03 ABCDEFG 2 A_WeaponReady(WRF_NOFIRE|WRF_NOSWITCH)
			SF03 H 0 A_PlaySound("SoF/Weapons/gpistol/ReloadOut",CHAN_AUTO)
			SF03 HIJKL 2 A_WeaponReady(WRF_NOFIRE|WRF_NOSWITCH)
			SF03 M 0 A_PlaySound("SoF/Weapons/gpistol/ReloadFIn",CHAN_AUTO)
			SF03 MN 2 A_WeaponReady(WRF_NOFIRE|WRF_NOSWITCH)
			SF0B A 0 ACS_NamedExecuteWithResult("SoF_Decorate",1,2)
			Goto ReadyLoop
		DryFire:
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",11,"DRYFIRE_2HANDS")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",10,"DRYFIRE_2HANDS")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",9,"DRYFIRE_1HAND")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",8,"DRYFIRE_2HANDS")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",7,"DRYFIRE_2HANDS")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",6,"DRYFIRE_2HANDS")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",5,"DRYFIRE_2HANDS")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",4,"DRYFIRE_1HAND")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",3,"DRYFIRE_2HANDS")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",2,"DRYFIRE_2HANDS")
			"####" "#" 0 A_JumpIfInventory("SoF_WeaponAnimState",1,"DRYFIRE_1HAND")
		DRYFIRE_1HAND:
			"####" "#" 2 A_WeaponReady(WRF_NOFIRE|WRF_NOSWITCH)
			SF11 A 0 A_PlaySound("SoF/Weapons/gpistol/Empty",CHAN_AUTO)
			SF11 ABC 2 A_WeaponReady(WRF_NOFIRE|WRF_NOSWITCH)
			SF0A A 0 A_JumpIfInventory("Clip",1,"Reload")
			SF0A A 8 A_WeaponReady(WRF_NOFIRE|WRF_NOSWITCH)
			Goto ReadyLoop
		DRYFIRE_2HANDS:
			"####" "#" 2 A_WeaponReady(WRF_NOFIRE|WRF_NOSWITCH)
			SF12 A 0 A_PlaySound("SoF/Weapons/gpistol/Empty",CHAN_AUTO)
			SF12 ABC 2 A_WeaponReady(WRF_NOFIRE|WRF_NOSWITCH)
			SF0B A 0 A_JumpIfInventory("Clip",1,"Reload")
			SF0B A 8 A_WeaponReady(WRF_NOFIRE|WRF_NOSWITCH)
			Goto ReadyLoop
		Spawn:
			SFPU A -1
			Stop
	}
}

Actor SoF_Pistol1_A_FireToken : Trigger
{
	States
	{
		Pickup:
			TNT1 A 0 A_SpawnItemEx("SoF_SmallCasing",cos(pitch)*8,4,38-(sin(pitch)*8),0.125*sin(pitch),frandom(0.75,1.25),abs(1*cos(pitch)),0,SXF_TRANSFERPITCH)
			TNT1 A 0 A_SpawnItemEx("SoF_MuzzleSmoke",cos(pitch)*8,3,38-(sin(pitch)*8),0.125*sin(pitch),0,abs(0.125*cos(pitch)))
			TNT1 A 0 A_PlaySound("SoF/Weapons/gpistol/Fire",CHAN_WEAPON)
			TNT1 A 0 A_AlertMonsters
			TNT1 A 0 A_FireBullets(4,4,-1,0,"SoF_Pistol1BulletPuff",FBF_NORANDOM)
			TNT1 A 0 A_TakeInventory("SoF_Pistol1Magazine",1)
			TNT1 A 0 ACS_NamedExecuteWithResult("GlobalRecoil",3,48)
			Stop
	}
}

