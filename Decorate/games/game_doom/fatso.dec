actor Fatso2 : SamsaraMonsterCyberneticTotalGib replaces Fatso
{
	Game Doom
	SpawnID 112
	Health 600
	Radius 48
	Height 64
	Mass 1000
	Speed 8
	PainChance 80
	Monster
	+FLOORCLIP
	+BOSSDEATH
	+DONTRIP
	SeeSound "fatso/sight"
	PainSound "fatso/pain"
	DeathSound "fatso/death"
	ActiveSound "fatso/active"
	Obituary "$OB_FATSO"
	Tag "Mancubus"
	Species "SamsaraMancubus"
