actor Goldeneye_ProximityMineStaticCoop : Goldeneye_ProximityMineStatic
{
    Species "Player"
    +FRIENDLY
    +THRUSPECIES
    States
    {
      Spawn:
        GE00 B 0
        GE00 B 0 A_PlaySound("Goldeneye/MinePlant",CHAN_VOICE)
        GE00 B 0 A_GiveInventory("ProximityTNTArmed",1)
        GE00 B 0 ACS_NamedExecuteWithResult("SamsaraProximityLooker")
        GE00 B 0 A_CheckFloor("SpawnLoopFloor")
        GE00 B 0 A_CheckCeiling("SpawnLoopCeiling")
        GE00 B 0 A_SetPitch(0)
        GE00 B 0 A_SetAngle(angle)
      SpawnLoop:
        GE00 B 0 A_ChangeFlag("FRIENDLY",true)
        GE00 B 70 A_SetPitch(90)
        Goto SpawnLoopContinue

      SpawnLoopFloor:
        GE00 B 0 A_ChangeFlag("FRIENDLY",true)
        GE00 B 70 A_SetPitch(0)
        Goto SpawnLoopFloorContinue

      SpawnLoopCeiling:
        GE00 C 0 A_ChangeFlag("FRIENDLY",true)
        GE00 C 70 A_SetPitch(180)
        Goto SpawnLoopCeilingContinue
    }
}

