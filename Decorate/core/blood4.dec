ACTOR Bolognese_XDeath5 : Bolognese_XDeath2
{
	DeathSound "bolognese/xdeath3"
	Scale 1.0
	Gravity 0.6
	Speed 4
	States
	{
	Spawn:
		XME5 A 3 A_SpawnItemEx("Bolognese_FlyingBloodTrail",0,0,0,0,0,0,random(0,360),SXF_NOCHECKPOSITION|SXF_TRANSFERTRANSLATION)
		Loop
	Death:
		TNT1 AAAAAAA 0 A_SpawnItemEx("Bolognese_FlyingBloodTrail",0,0,0,0,0,0,random(0,180),SXF_NOCHECKPOSITION|SXF_TRANSFERTRANSLATION)
		XME5 B 4 A_SpawnItemEx("Bolognese_BloodSpot",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION|SXF_TRANSFERTRANSLATION)
	Living:
		XME5 B 800
		"####" B 0 A_JumpIf(ACS_NamedExecuteWithResult("SamsaraDecorate",31) == 1, "Living")
		"####" BBBBBBBBBB 1 A_FadeOut(0.1)
		Stop
	}
}

