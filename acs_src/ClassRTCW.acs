Script "RTCW_TeslaPush" (void)
{
	int puffTid = UniqueTid();
	Thing_ChangeTid(0,puffTid);
	int angle = GetActorAngle(0) - 0.5;
	
	if(!SetActivator(0,AAPTR_Tracer))
		terminate;
		
	if(GetActorProperty(0,APROP_Mass) < 500)
	{
		ThrustThing(angle>>8,2,0,0);	
		ThrustThingZ(0,8,0,1);
	}
	
	if(ClassifyActor(0) & ACTOR_DEAD)
	{
		PlaySound(0,"RTCW/Tesla/Death",CHAN_7,1.0,0,ATTN_NORM);
		SetActivator(puffTid);
		SetActivatorToTarget(0);
		GiveInventory("RTCW_TeslaShake",1);
	}
}