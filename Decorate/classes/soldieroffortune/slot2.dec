Actor SoF_AssaultRifle_A_FireToken : Trigger
{
	States
	{
		Pickup:
			TNT1 A 0 A_SpawnItemEx("SoF_SmallCasing",cos(pitch)*8,4,38-(sin(pitch)*8),0.125*sin(pitch),frandom(0.75,1.25),abs(1*cos(pitch)),0,SXF_TRANSFERPITCH)
			TNT1 A 0 A_SpawnItemEx("SoF_MuzzleSmoke2",cos(pitch)*8,2.5,39-(sin(pitch)*8),frandom(0.05,0.1)*sin(pitch),frandom(-0.05,0.05),abs(frandom(0.05,0.1)*cos(pitch)))
			TNT1 A 0 A_PlaySound("SoF/Weapons/assault/Fire",CHAN_WEAPON)
			TNT1 A 0 A_AlertMonsters
			TNT1 A 0 A_FireBullets(2,2,-1,0,"SoF_AssaultRifleBulletPuff",FBF_NORANDOM)
			TNT1 A 0 A_TakeInventory("SoF_AssaultRifleMagazine",1)
			TNT1 A 0 ACS_NamedExecuteWithResult("GlobalRecoil",2,48)
			Stop
	}
}

