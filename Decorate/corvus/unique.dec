        TNT1 A 0 A_FireCustomMissile("SamsaraCorvusEggFXPD",7.5,false,0,0,1)
        TNT1 A 0 A_FireCustomMissile("SamsaraCorvusEggFXPD",15,false,0,0,1)
        stop

      CoopUse:
        TNT1 A 0 A_FireCustomMissile("SamsaraCorvusEggFXCoop",-15,false,0,0,1)
        TNT1 A 0 A_FireCustomMissile("SamsaraCorvusEggFXCoop",-7.5,false,0,0,1)
