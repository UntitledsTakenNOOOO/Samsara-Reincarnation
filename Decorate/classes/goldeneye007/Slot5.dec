actor Goldeneye_RocketLauncherPickedUpX : MapCoordinate {}
actor Goldeneye_RocketLauncherPickedUpY : MapCoordinate {}
actor Goldeneye_RocketLauncherPickedUpZ : MapCoordinate {}
actor Goldeneye_RocketLauncherLastPickupTime : Counter { Inventory.InterHubAmount 0 }
actor Goldeneye_RocketLauncherLevelLimiter : Boolean {}

actor Goldeneye_RocketLauncher : SamsaraGoldenEyeWeapon
{
    Inventory.PickupMessage "Picked up a rocket launcher."
    Inventory.Icon "GEWRCK12"
    Weapon.SlotNumber 5
    Weapon.SlotPriority 0
    Weapon.SelectionOrder 5
    Weapon.AmmoType1 "Goldeneye_RocketLauncherMagazine"
    Weapon.AmmoUse1 1
    Weapon.AmmoType2 "RocketAmmo"
    Weapon.AmmoGive2 3
    Obituary "%o was blown up by %k's rocket launcher."
    Tag "Rocket Launcher"
    +EXPLOSIVE
    States
    {
      Spawn:
        PIST A -1
        stop

      // Single
      Ready:
        TNT1 A 0 A_JumpIfInventory("BondWatchQueue",1,"WatchRaise")
        TNT1 A 0 A_JumpIfInventory("BondWatchToken",1,"WatchReady")
        TNT1 A 0 A_JumpIfInventory("BondAltToggle",1,"DualWield")
      ReadyStart:
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"ReadyReloadable")
      ReadyNonReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"ReadyNonReloadableAmmo")
        TNT1 A 0 A_JumpIfInventory("RocketAmmo",1,"ReadyNonReloadableAmmo")
        goto ReadyNonReloadableNoAmmo

      ReadyNonReloadableNoAmmo:
        GE00 A 1 A_WeaponReady
        goto ReadyNoAmmoCheck

      ReadyNonReloadableAmmo:
        GE02 A 1 A_WeaponReady
        goto ReadyAmmoCheck

      ReadyReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,"ReadyReloadableAmmo")
        TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherMagazine",1,"ReadyReloadableAmmo")
        goto ReadyReloadableNoAmmo

      ReadyReloadableNoAmmo:
        GE00 A 1 A_WeaponReady(WRF_ALLOWRELOAD)
        goto ReadyAmmoCheck

      ReadyReloadableAmmo:
        GE02 A 1 A_WeaponReady(WRF_ALLOWRELOAD)
        goto ReadyAmmoCheck

      ReadyNoAmmoCheck:
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"ReadyAmmoCheck")
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",1,1)
        goto Ready
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"ReloadStart")
        TNT1 A 0 A_JumpIfInventory("RocketAmmo",1,"ReloadStart")
        goto Ready

      ReadyAmmoCheck:
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,1)
        goto Ready
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,"Ready")
        TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherMagazine",1,"Ready")
        TNT1 A 0 A_JumpIfInventory("SamsaraSynthFireLeft",1,"Ready")
        goto ReloadCheck

      Deselect:
        TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherDualToken",1,2)
        TNT1 A 0 A_GunFlash("DualForceRemoval",GFF_NOEXTCHANGE)
        goto DeselectLoop
        TNT1 A 0 A_JumpIfInventory("BondDualWieldToken",1,1)
        goto DeselectLoop
        TNT1 A 0 A_GunFlash("DeselectDual",GFF_NOEXTCHANGE)
      DeselectLoop:
        TNT1 A 0 A_TakeInventory("BondFireDelay")
        TNT1 A 0 A_TakeInventory("BondAlternateFire")
        TNT1 A 0 A_TakeInventory("BondDualWieldToken")
        TNT1 A 0 A_TakeInventory("BondAltToggle")
        TNT1 A 0 A_TakeInventory("BondZoomToken")
        TNT1 A 0 A_TakeInventory("BondZoomAmountModifyActive")
        TNT1 A 0 A_TakeInventory("BondDualWieldSpamProtector")
        TNT1 A 0 A_TakeInventory("BondDryFireSpamProtector1")
        TNT1 A 0 A_TakeInventory("BondDryFireSpamProtector2")
        TNT1 A 0 A_TakeInventory("BondAltActivationToken")
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"DeselectLoopReloadable")
      DeselectLoopNonReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"DeselectLoopAmmo")
        TNT1 A 0 A_JumpIfInventory("RocketAmmo",1,"DeselectLoopAmmo")
        goto DeselectLoopNoAmmo

      DeselectLoopReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,"DeselectLoopAmmo")
        TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherMagazine",1,"DeselectLoopAmmo")
        goto DeselectLoopNoAmmo

      DeselectLoopNoAmmo:
        GE00 A 1 A_Lower
        goto DeselectLoop

      DeselectLoopAmmo:
        GE02 A 1 A_Lower
        goto DeselectLoop

      Select:
        TNT1 A 0 A_GiveInventory("SamsaraGoldenEyeWeaponSwitchSoundPlayer")
      SelectDualCheck:
        TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherDualToken",1,2)
        TNT1 A 0 A_GunFlash("DualForceRemoval",GFF_NOEXTCHANGE)
        goto SelectLoop
        TNT1 A 0 A_JumpIfInventory("BondDualWieldToken",1,1)
        goto SelectLoop
        TNT1 A 0 A_GunFlash("SelectDual",GFF_NOEXTCHANGE)
      SelectLoop:
        TNT1 A 0 A_TakeInventory("Goldeneye_UnarmedToken")
        TNT1 A 0 A_TakeInventory("Goldeneye_PP7Token")
        TNT1 A 0 A_TakeInventory("Goldeneye_KnifeToken")
        TNT1 A 0 A_TakeInventory("Goldeneye_KF7Token")
        TNT1 A 0 A_TakeInventory("Goldeneye_AutoShotgunToken")
        TNT1 A 0 A_TakeInventory("Goldeneye_AR33Token")
        TNT1 A 0 A_GiveInventory("Goldeneye_RocketLauncherToken")
        TNT1 A 0 A_TakeInventory("Goldeneye_RCP90Token")
        TNT1 A 0 A_TakeInventory("Goldeneye_LaserToken")
        TNT1 A 0 A_TakeInventory("Goldeneye_GoldenGunToken")
        TNT1 A 0 A_TakeInventory("Goldeneye_SniperToken")
        TNT1 A 0 A_TakeInventory("Goldeneye_D5KToken")
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"SelectLoopReloadable")
      SelectLoopNonReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"SelectLoopAmmo")
        TNT1 A 0 A_JumpIfInventory("RocketAmmo",1,"SelectLoopAmmo")
        goto SelectLoopNoAmmo

      SelectLoopReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,"SelectLoopAmmo")
        TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherMagazine",1,"SelectLoopAmmo")
        goto SelectLoopNoAmmo

      SelectLoopNoAmmo:
        GE00 A 1 A_Raise
        goto SelectDualCheck

      SelectLoopAmmo:
        GE02 A 1 A_Raise
        goto SelectDualCheck

      Fire:
        GE02 A 0 A_JumpIfInventory("SamsaraSynthFireRight",1,1)
        goto FireAltDualCheck
        GE02 A 0 A_JumpIfInventory("BondAltActivationToken",1,"FireAltDualCheck")
        GE02 A 0 A_TakeInventory("BondAltToggle")
        GE02 A 0 A_GiveInventory("BondAltActivationToken")
        GE02 A 0 ACS_NamedExecuteAlways("BondAltFire")
        loop

      FireAltDualCheck:
        GE02 A 0 A_JumpIfInventory("BondAltToggle",1,"DualWieldFire")
      FireCheckDualOrder:
        GE02 A 0 A_JumpIfInventory("BondDualWieldToken",1,1)
        goto FireAmmoCheck
        GE02 A 0 A_JumpIfInventory("BondAlternateFire",1,"Ready")
        goto FireAmmoCheck

      FireAmmoCheck:
        GE02 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"FireAmmoCheckReloadable")
      FireAmmoCheckNonReloadable:
        GE02 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"FireStart")
        GE02 A 0 A_JumpIfInventory("RocketAmmo",1,"FireStart")
        goto FireDry

      FireAmmoCheckReloadable:
        GE02 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,"FireStart")
        GE02 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherMagazine",1,"FireStart")
        goto FireDry

      FireDry:
        "####" "#" 0 A_JumpIfInventory("BondDryFireSpamProtector1",1,"FireDryBlocker")
        "####" "#" 0 A_GiveInventory("SamsaraGoldenEyeWeaponDrySoundPlayer")
        "####" "#" 0 ACS_NamedExecuteAlways("BondDryFireSpamBlocker_Primary")
        "####" "#" 0 A_GiveInventory("BondDryFireSpamProtector1")
	  FireDryBlocker:
		"####" "#" 0 A_JumpIfInventory("BondDualWieldToken",1,1)
        goto FireFinish
        "####" "#" 0 A_GiveInventory("BondAlternateFire")
        goto FireFinish

      FireStart:
        GE02 A 0 A_GiveInventory("BondFiringToken")
        GE02 A 0 A_AlertMonsters
        GE02 A 1 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        GE00 A 0 A_GiveInventory("SamsaraGoldenEyeRocketLauncherSingleAttackHandler")
        GE00 A 0 A_TakeInventory("BondFiringToken")
        GE00 A 0 ACS_NamedExecuteWithResult("SamsaraSemiAuto")
        GE00 A 1 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        GE00 A 0 A_JumpIfInventory("SamsaraFiringFullAuto",1,2)
        GE00 A 0 A_ReFire("FireFinish")
        GE00 A 1 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        GE00 A 0 A_JumpIfInventory("SamsaraFiringFullAuto",1,2)
        GE00 A 0 A_ReFire("FireFinish")
        GE00 A 1 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        GE00 A 0 A_JumpIfInventory("SamsaraFiringFullAuto",1,2)
        GE00 A 0 A_ReFire("FireFinish")
        GE00 A 1 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        GE00 A 0 A_JumpIfInventory("SamsaraFiringFullAuto",1,2)
        GE00 A 0 A_ReFire("FireFinish")
        GE00 A 1 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        GE00 A 0 A_JumpIfInventory("SamsaraFiringFullAuto",1,2)
        GE00 A 0 A_ReFire("FireFinish")
        GE00 A 1 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        GE00 A 0 A_JumpIfInventory("SamsaraFiringFullAuto",1,2)
        GE00 A 0 A_ReFire("FireFinish")
        GE00 A 1 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        GE00 A 0 A_JumpIfInventory("SamsaraFiringFullAuto",1,2)
        GE00 A 0 A_ReFire("FireFinish")
        GE00 A 1 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        GE00 A 0 A_JumpIfInventory("SamsaraFiringFullAuto",1,2)
        GE00 A 0 A_ReFire("FireFinish")
        GE00 A 1 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        GE00 A 0 A_JumpIfInventory("SamsaraFiringFullAuto",1,2)
        GE00 A 0 A_ReFire("FireFinish")
        GE00 A 1 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        GE00 A 0 A_JumpIfInventory("SamsaraFiringFullAuto",1,2)
        GE00 A 0 A_ReFire("FireFinish")
        GE00 A 1 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        goto FireFinish

      FireFinish:
		TNT1 A 0 A_TakeInventory("SamsaraFiringFullAuto")
        TNT1 A 0 A_TakeInventory("BondFiringToken")
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"Ready")
        goto ReadyNoAmmoCheck

      Reload:
        TNT1 A 0 A_JumpIfInventory("SamsaraSynthFireLeft",1,"ReloadCheck")
        TNT1 A 0 A_JumpIfInventory("BondDualWieldToken",1,1)
        goto ReloadCheck
        TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherDualMagazine",0,"ReloadCheck")
        TNT1 A 0 A_JumpIfInventory("BondDualReloadSpamProtector",1,"ReloadCheck")
        TNT1 A 0 A_GunFlash("ReloadCheckDual",GFF_NOEXTCHANGE)
        goto ReloadCheck

      ReloadCheck:
        TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherMagazine",0,"Ready")
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,2)
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"ReloadStart")
        TNT1 A 0 A_JumpIfInventory("RocketAmmo",1,"ReloadStart")
        goto Ready

      ReloadStart:
        TNT1 A 0 A_GiveInventory("SamsaraGoldenEyeWeaponReloadSoundPlayer")
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,1)
        goto ReloadStartNoAmmo
        TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherMagazine",1,"ReloadStartAmmo")
        goto ReloadStartNoAmmo

      ReloadStartNoAmmo:
        GE01 ABCDEFGHI 1 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        goto ReloadAmmoModeCheck

      ReloadStartAmmo:
        GE03 ABCDEFGHI 1 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        goto ReloadAmmoModeCheck

      ReloadAmmoModeCheck:
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"ReloadAmmo")
        goto ReloadFinish

      ReloadFinish:
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"ReloadFinishReloadable")
      ReloadFinishNonReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"ReloadFinishAmmo")
        TNT1 A 0 A_JumpIfInventory("RocketAmmo",1,"ReloadFinishAmmo")
        goto ReloadFinishNoAmmo

      ReloadFinishReloadable:
        TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherMagazine",1,"ReloadFinishAmmo")
        goto ReloadFinishNoAmmo

      ReloadFinishNoAmmo:
        GE01 IHGFEDCBA 1 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        goto Ready

      ReloadFinishAmmo:
        GE03 IHGFEDCBA 1 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        goto Ready

      ReloadAmmo:
        TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherMagazine",0,"ReloadFinish")
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,2)
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,2)
        TNT1 A 0 A_JumpIfInventory("RocketAmmo",1,1)
        goto ReloadFinish
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,2)
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,2)
        TNT1 A 0 A_TakeInventory("RocketAmmo",1)
        TNT1 A 0 A_GiveInventory("Goldeneye_RocketLauncherMagazine")
        loop

      // Dual Init
      AltFire:
        TNT1 A 0 A_JumpIfInventory("BondAltActivationToken",1,"Ready")
        TNT1 A 0 A_TakeInventory("BondAltToggle")
        TNT1 A 0 A_GiveInventory("BondAltActivationToken")
        TNT1 A 0 ACS_NamedExecuteAlways("BondAltFire")
        goto Ready

      DualUnwield:
        TNT1 A 0 A_TakeInventory("BondAlternateFire")
        TNT1 A 0 A_JumpIfInventory("BondDualWieldSpamProtector",1,5)
        TNT1 A 0 A_TakeInventory("BondDualWieldToken")
        TNT1 A 0 A_TakeInventory("BondDryFireSpamProtector1")
        TNT1 A 0 A_TakeInventory("BondDryFireSpamProtector2")
        TNT1 A 0 A_GunFlash("DualLower",GFF_NOEXTCHANGE)
        TNT1 A 0 A_ZoomFactor(1)
        goto ReadyStart

      DualUnwieldFire:
        GE02 A 0 A_TakeInventory("BondAlternateFire")
        GE02 A 0 A_JumpIfInventory("BondDualWieldSpamProtector",1,5)
        GE02 A 0 A_TakeInventory("BondDualWieldToken")
        GE02 A 0 A_TakeInventory("BondDryFireSpamProtector1")
        GE02 A 0 A_TakeInventory("BondDryFireSpamProtector2")
        GE02 A 0 A_GunFlash("DualLower",GFF_NOEXTCHANGE)
        GE02 A 0 A_ZoomFactor(1)
        goto FireCheckDualOrder

      DualLower:
        TNT1 A 0 A_TakeInventory("BondDualReloadSpamProtector")
        TNT1 A 0 A_GiveInventory("BondDualWieldSpamProtector")
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"DualLowerReloadable")
      DualLowerNonReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"DualLowerAmmo")
        TNT1 A 0 A_JumpIfInventory("RocketAmmo",1,"DualLowerAmmo")
        goto DualLowerNoAmmo

      DualLowerReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,"DualLowerAmmo")
        TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherDualMagazine",1,"DualLowerAmmo")
        goto DualLowerNoAmmo

      DualLowerNoAmmo:
        GE21 ABCDEFGHI 1
        goto DualLowerFinish

      DualLowerAmmo:
        GE23 ABCDEFGHI 1
        goto DualLowerFinish

      DualLowerFinish:
        TNT1 A 0 A_TakeInventory("BondDualWieldSpamProtector")
        stop

      DualWield:
        TNT1 A 0 A_TakeInventory("BondAltActivationToken")
        TNT1 A 0 A_TakeInventory("BondAltToggle")
        TNT1 A 0 A_TakeInventory("BondZoomToken")
        TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherDualToken",1,1)
        goto ReadyStart
        TNT1 A 0 A_JumpIfInventory("BondDualWieldToken",1,"DualUnwield")
        TNT1 A 0 A_JumpIfInventory("BondDualWieldSpamProtector",1,3)
        TNT1 A 0 A_GiveInventory("BondDualWieldToken")
        TNT1 A 0 A_GunFlash("DualRaise",GFF_NOEXTCHANGE)
        TNT1 A 0 A_ZoomFactor(1)
        goto ReadyStart

      DualWieldFire:
        GE02 A 0 A_TakeInventory("BondAltActivationToken")
        GE02 A 0 A_TakeInventory("BondAltToggle")
        GE02 A 0 A_TakeInventory("BondZoomToken")
        GE02 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherDualToken",1,1)
        goto FireCheckDualOrder
        GE02 A 0 A_JumpIfInventory("BondDualWieldToken",1,"DualUnwieldFire")
        GE02 A 0 A_JumpIfInventory("BondDualWieldSpamProtector",1,3)
        GE02 A 0 A_GiveInventory("BondDualWieldToken")
        GE02 A 0 A_GunFlash("DualRaise",GFF_NOEXTCHANGE)
        GE02 A 0 A_ZoomFactor(1)
        goto FireCheckDualOrder

      DualRaise:
        TNT1 A 0 A_TakeInventory("BondDualReloadSpamProtector")
        TNT1 A 0 A_GiveInventory("BondDualWieldSpamProtector")
        TNT1 A 0 A_GiveInventory("SamsaraGoldenEyeWeaponSwitchSoundPlayer")
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"DualRaiseReloadable")
      DualRaiseNonReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"DualRaiseAmmo")
        TNT1 A 0 A_JumpIfInventory("RocketAmmo",1,"DualRaiseAmmo")
        goto DualRaiseNoAmmo

      DualRaiseReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,"DualRaiseAmmo")
        TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherDualMagazine",1,"DualRaiseAmmo")
        goto DualRaiseNoAmmo

      DualRaiseNoAmmo:
        GE21 IHGFEDCBA 1
        goto DualRaiseFinish

      DualRaiseAmmo:
        GE23 IHGFEDCBA 1
        goto DualRaiseFinish

      DualRaiseFinish:
        TNT1 A 0 A_TakeInventory("BondDualWieldSpamProtector")
        goto ReadyStartDual

      // Dual
      ReadyDual:
        TNT1 A 0 A_JumpIfInventory("BondAlternateFire",1,1)
        goto ReadyStartDual
        TNT1 A 0 A_JumpIfInventory("SamsaraSynthFireLeft",1,"FireDual")
      ReadyStartDual:
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"ReadyDualReloadable")
      ReadyDualNonReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"ReadyDualNonReloadableAmmo")
        TNT1 A 0 A_JumpIfInventory("RocketAmmo",1,"ReadyDualNonReloadableAmmo")
        goto ReadyDualNonReloadableNoAmmo

      ReadyDualNonReloadableNoAmmo:
        GE20 A 1
        goto ReadyNoAmmoCheckDual

      ReadyDualNonReloadableAmmo:
        GE22 A 1
        goto ReadyAmmoCheckDual

      ReadyDualReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,"ReadyDualReloadableAmmo")
        TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherDualMagazine",1,"ReadyDualReloadableAmmo")
        goto ReadyDualReloadableNoAmmo

      ReadyDualReloadableNoAmmo:
        GE20 A 1
        goto ReadyAmmoCheckDual

      ReadyDualReloadableAmmo:
        GE22 A 1
        goto ReadyAmmoCheckDual

      ReadyNoAmmoCheckDual:
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"ReadyAmmoCheckDual")
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",1,1)
        goto ReadyDual
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"ReloadStartDual")
        TNT1 A 0 A_JumpIfInventory("RocketAmmo",1,"ReloadStartDual")
        goto ReadyDual

      ReadyAmmoCheckDual:
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,1)
        goto ReadyDual
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,"ReadyDual")
        TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherDualMagazine",1,"ReadyDual")
        TNT1 A 0 A_JumpIfInventory("SamsaraSynthFireLeft",1,"ReadyDual")
        goto ReloadCheckDual

      DeselectDual:
        TNT1 A 0 A_TakeInventory("BondDualReloadSpamProtector")
        TNT1 A 0 A_TakeInventory("BondDualWieldSpamProtector")
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"DeselectDualReloadable")
      DeselectDualNonReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"DeselectDualAmmo")
        TNT1 A 0 A_JumpIfInventory("RocketAmmo",1,"DeselectDualAmmo")
        goto DeselectDualNoAmmo

      DeselectDualReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,"DeselectDualAmmo")
        TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherDualMagazine",1,"DeselectDualAmmo")
        goto DeselectDualNoAmmo

      DeselectDualNoAmmo:
        GE20 AAAAAAAAAAAAAAAA 1
        stop

      DeselectDualAmmo:
        GE22 AAAAAAAAAAAAAAAA 1
        stop

      SelectDual:
        TNT1 A 0 A_TakeInventory("BondDualReloadSpamProtector")
        TNT1 A 0 A_TakeInventory("BondDualWieldSpamProtector")
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"SelectDualReloadable")
      SelectDualNonReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"SelectDualAmmo")
        TNT1 A 0 A_JumpIfInventory("RocketAmmo",1,"SelectDualAmmo")
        goto SelectDualNoAmmo

      SelectDualReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,"SelectDualAmmo")
        TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherDualMagazine",1,"SelectDualAmmo")
        goto SelectDualNoAmmo

      SelectDualNoAmmo:
        GE20 A 1
        goto ReadyStartDual

      SelectDualAmmo:
        GE22 A 1
        goto ReadyStartDual

      FireDual:
        GE22 A 0 A_JumpIfInventory("BondAlternateFire",1,"FireAmmoCheckDual")
        goto ReadyDual

      FireAmmoCheckDual:
        GE22 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"FireAmmoCheckDualReloadable")
      FireAmmoCheckDualNonReloadable:
        GE22 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"FireStartDual")
        GE22 A 0 A_JumpIfInventory("RocketAmmo",1,"FireStartDual")
        goto FireDryDual

      FireAmmoCheckDualReloadable:
        GE22 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,"FireStartDual")
        GE22 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherDualMagazine",1,"FireStartDual")
        goto FireDryDual

      FireDryDual:
        "####" "#" 0 A_JumpIfInventory("BondDryFireSpamProtector2",1,"FireDryBlockerDual")
        "####" "#" 0 A_GiveInventory("SamsaraGoldenEyeWeaponDrySoundPlayer")
        "####" "#" 0 ACS_NamedExecuteAlways("BondDryFireSpamBlocker_Secondary")
        "####" "#" 0 A_GiveInventory("BondDryFireSpamProtector2")
	  FireDryBlockerDual:
		"####" "#" 0 A_TakeInventory("BondAlternateFire")
        goto FireFinishDual

      FireStartDual:
        GE22 A 0 A_GiveInventory("BondFiringToken")
        GE22 A 1 A_AlertMonsters
        GE20 A 0 A_GiveInventory("SamsaraGoldenEyeRocketLauncherDualAttackHandler")
        GE20 A 0 A_TakeInventory("BondFiringToken")
        GE20 A 1 ACS_NamedExecuteWithResult("SamsaraSemiAuto2")
        GE20 A 0 A_JumpIfInventory("SamsaraFiringFullAuto2",1,2)
        GE20 A 0 A_JumpIfInventory("SamsaraSynthFireLeft",1,"FireFinishDual")
        GE20 A 1
        GE20 A 0 A_JumpIfInventory("SamsaraFiringFullAuto2",1,2)
        GE20 A 0 A_JumpIfInventory("SamsaraSynthFireLeft",1,"FireFinishDual")
        GE20 A 1
        GE20 A 0 A_JumpIfInventory("SamsaraFiringFullAuto2",1,2)
        GE20 A 0 A_JumpIfInventory("SamsaraSynthFireLeft",1,"FireFinishDual")
        GE20 A 1
        GE20 A 0 A_JumpIfInventory("SamsaraFiringFullAuto2",1,2)
        GE20 A 0 A_JumpIfInventory("SamsaraSynthFireLeft",1,"FireFinishDual")
        GE20 A 1
        GE20 A 0 A_JumpIfInventory("SamsaraFiringFullAuto2",1,2)
        GE20 A 0 A_JumpIfInventory("SamsaraSynthFireLeft",1,"FireFinishDual")
        GE20 A 1
        GE20 A 0 A_JumpIfInventory("SamsaraFiringFullAuto2",1,2)
        GE20 A 0 A_JumpIfInventory("SamsaraSynthFireLeft",1,"FireFinishDual")
        GE20 A 1
        GE20 A 0 A_JumpIfInventory("SamsaraFiringFullAuto2",1,2)
        GE20 A 0 A_JumpIfInventory("SamsaraSynthFireLeft",1,"FireFinishDual")
        GE20 A 1
        GE20 A 0 A_JumpIfInventory("SamsaraFiringFullAuto2",1,2)
        GE20 A 0 A_JumpIfInventory("SamsaraSynthFireLeft",1,"FireFinishDual")
        GE20 A 1
        GE20 A 0 A_JumpIfInventory("SamsaraFiringFullAuto2",1,2)
        GE20 A 0 A_JumpIfInventory("SamsaraSynthFireLeft",1,"FireFinishDual")
        GE20 A 1
        GE20 A 0 A_JumpIfInventory("SamsaraFiringFullAuto2",1,2)
        GE20 A 0 A_JumpIfInventory("SamsaraSynthFireLeft",1,"FireFinishDual")
        GE20 A 1
        goto FireFinishDual

      FireFinishDual:
		TNT1 A 0 A_TakeInventory("SamsaraFiringFullAuto2")
        TNT1 A 0 A_TakeInventory("BondFiringToken")
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"ReadyDual")
        goto ReadyNoAmmoCheckDual

      ReloadCheckDual:
        TNT1 A 0 A_TakeInventory("BondDualWieldSpamProtector")
        TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherDualMagazine",0,"ReadyDual")
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,2)
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"ReloadStartDual")
        TNT1 A 0 A_JumpIfInventory("RocketAmmo",1,"ReloadStartDual")
        goto ReadyDual

      ReloadStartDual:
        TNT1 A 0 A_GiveInventory("BondDualReloadSpamProtector")
        TNT1 A 0 A_GiveInventory("SamsaraGoldenEyeWeaponReloadSoundPlayer")
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,1)
        goto ReloadStartDualNoAmmo
        TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherDualMagazine",1,"ReloadStartDualAmmo")
        goto ReloadStartDualNoAmmo

      ReloadStartDualNoAmmo:
        GE21 ABCDEFGHI 1
        goto ReloadAmmoModeCheckDual

      ReloadStartDualAmmo:
        GE23 ABCDEFGHI 1
        goto ReloadAmmoModeCheckDual

      ReloadAmmoModeCheckDual:
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"ReloadAmmoDual")
        goto ReloadFinishDual

      ReloadFinishDual:
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"ReloadFinishDualReloadable")
      ReloadFinishDualNonReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"ReloadFinishDualAmmo")
        TNT1 A 0 A_JumpIfInventory("RocketAmmo",1,"ReloadFinishDualAmmo")
        goto ReloadFinishDualNoAmmo

      ReloadFinishDualReloadable:
        TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherDualMagazine",1,"ReloadFinishDualAmmo")
        goto ReloadFinishDualNoAmmo

      ReloadFinishDualNoAmmo:
        GE21 IHGFEDCBA 1
        goto ReloadFinishClearDual

      ReloadFinishDualAmmo:
        GE23 IHGFEDCBA 1
        goto ReloadFinishClearDual

      ReloadFinishClearDual:
        TNT1 A 0 A_TakeInventory("BondDualReloadSpamProtector")
        goto ReadyDual

      ReloadAmmoDual:
        TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherDualMagazine",0,"ReloadFinishDual")
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,2)
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,2)
        TNT1 A 0 A_JumpIfInventory("RocketAmmo",1,1)
        goto ReloadFinishDual
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,2)
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,2)
        TNT1 A 0 A_TakeInventory("RocketAmmo",1)
        TNT1 A 0 A_GiveInventory("Goldeneye_RocketLauncherDualMagazine")
        loop

      // Watch
      WatchReady:
        TNT1 A 0 A_JumpIfInventory("BondWatchQueue",1,"WatchRaise")
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"WatchReadyReloadable")
      WatchReadyNonReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"WatchReadyAmmo")
        TNT1 A 0 A_JumpIfInventory("RocketAmmo",1,"WatchReadyAmmo")
        goto WatchReadyNoAmmo

      WatchReadyReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,"WatchReadyAmmo")
        TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherMagazine",1,"WatchReadyAmmo")
        goto WatchReadyNoAmmo

      WatchReadyNoAmmo:
        GE14 T 4 A_WeaponReady(WRF_NOBOB|WRF_NOFIRE|WRF_DISABLESWITCH)
        goto WatchReady

      WatchReadyAmmo:
        GE34 T 4 A_WeaponReady(WRF_NOBOB|WRF_NOFIRE|WRF_DISABLESWITCH)
        goto WatchReady

      WatchLower:
        TNT1 A 0 A_TakeInventory("BondWatchActivate")
        TNT1 A 0 A_GiveInventory("BondWatchUIDeinit")
      WatchLowerStart:
        TNT1 A 0 ACS_NamedExecuteAlways("BondWatchLookUpAndDown")
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"WatchLowerStartReloadable")
      WatchLowerStartNonReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"WatchLowerStartAmmo")
        TNT1 A 0 A_JumpIfInventory("RocketAmmo",1,"WatchLowerStartAmmo")
        goto WatchLowerStartNoAmmo

      WatchLowerStartReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,"WatchLowerStartAmmo")
        TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherMagazine",1,"WatchLowerStartAmmo")
        goto WatchLowerStartNoAmmo

      WatchLowerStartNoAmmo:
        GE14 T 12 A_WeaponReady(WRF_NOBOB|WRF_NOFIRE|WRF_DISABLESWITCH)
        GE14 SRQPONMLKJIHGFEDCBA 2 A_WeaponReady(WRF_NOBOB|WRF_NOFIRE|WRF_DISABLESWITCH)
        goto WatchLowerDualCheck

      WatchLowerStartAmmo:
        GE34 T 12 A_WeaponReady(WRF_NOBOB|WRF_NOFIRE|WRF_DISABLESWITCH)
        GE34 SRQPONMLKJIHGFEDCBA 2 A_WeaponReady(WRF_NOBOB|WRF_NOFIRE|WRF_DISABLESWITCH)
        goto WatchLowerDualCheck

      WatchLowerDualCheck:
        TNT1 A 0 A_JumpIfInventory("BondDualWieldToken",1,1)
        goto WatchLowerFinish
        TNT1 A 0 A_GunFlash("DualRaise",GFF_NOEXTCHANGE)
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"WatchLowerDualCheckReloadable")
      WatchLowerDualCheckNonReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"WatchLowerDualCheckAmmo")
        TNT1 A 0 A_JumpIfInventory("RocketAmmo",1,"WatchLowerDualCheckAmmo")
        goto WatchLowerDualCheckNoAmmo

      WatchLowerDualCheckReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,"WatchLowerDualCheckAmmo")
        TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherMagazine",1,"WatchLowerDualCheckAmmo")
        goto WatchLowerDualCheckNoAmmo

      WatchLowerDualCheckNoAmmo:
        GE00 A 12 A_WeaponReady(WRF_NOFIRE|WRF_DISABLESWITCH)
        goto WatchLowerFinish

      WatchLowerDualCheckAmmo:
        GE02 A 12 A_WeaponReady(WRF_NOFIRE|WRF_DISABLESWITCH)
        goto WatchLowerFinish

      WatchLowerFinish:
        TNT1 A 0 A_TakeInventory("BondWatchQueue")
        TNT1 A 0 SetPlayerProperty(false,false,PROP_FROZEN)
        TNT1 A 0 A_JumpIfInventory("DMModeOn",1,3)
        TNT1 A 0 A_JumpIfInventory("CoopModeOn",1,2)
        TNT1 A 0 A_TakeInventory("PowerTimeFreezer")
        TNT1 A 0 A_TakeInventory("BondWatchToken")
        TNT1 A 0 A_TakeInventory("BondWatchUIInit")
        TNT1 A 0 ACS_NamedExecuteAlways("BondWeaponSwitch")
        goto BackToReady

      WatchRaise:
        TNT1 A 0 A_ZoomFactor(1)
        TNT1 A 0 A_TakeInventory("BondAltToggle")
        TNT1 A 0 A_TakeInventory("BondZoomToken")
        TNT1 A 0 A_TakeInventory("BondZoomAmountModifyActive")
        TNT1 A 0 A_JumpIfInventory("BondWatchToken",1,"WatchLower")
        TNT1 A 0 A_GiveInventory("BondWatchActivate")
      WatchRaiseDualCheck:
        TNT1 A 0 A_JumpIfInventory("BondDualWieldToken",1,1)
        goto WatchRaiseStart
        TNT1 A 0 A_GunFlash("DualLower",GFF_NOEXTCHANGE)
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"WatchRaiseDualCheckReloadable")
      WatchRaiseDualCheckNonReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"WatchRaiseDualCheckAmmo")
        TNT1 A 0 A_JumpIfInventory("RocketAmmo",1,"WatchRaiseDualCheckAmmo")
        goto WatchRaiseDualCheckNoAmmo

      WatchRaiseDualCheckReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,"WatchRaiseDualCheckAmmo")
        TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherMagazine",1,"WatchRaiseDualCheckAmmo")
        goto WatchRaiseDualCheckNoAmmo

      WatchRaiseDualCheckNoAmmo:
        GE00 A 12 A_WeaponReady(WRF_NOFIRE|WRF_DISABLESWITCH)
        goto WatchRaiseStart

      WatchRaiseDualCheckAmmo:
        GE02 A 12 A_WeaponReady(WRF_NOFIRE|WRF_DISABLESWITCH)
        goto WatchRaiseStart

      WatchRaiseStart:
        TNT1 A 0 A_TakeInventory("BondPitchTokens")
        TNT1 A 0 A_TakeInventory("BondAngleTokens")
        TNT1 A 0 SetPlayerProperty(false,true,PROP_FROZEN)
        TNT1 A 0 ACS_NamedExecuteAlways("BondWatchLookUpAndDown")
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"WatchRaiseStartReloadable")
      WatchRaiseStartNonReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"WatchRaiseStartAmmo")
        TNT1 A 0 A_JumpIfInventory("RocketAmmo",1,"WatchRaiseStartAmmo")
        goto WatchRaiseStartNoAmmo

      WatchRaiseStartReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,"WatchRaiseStartAmmo")
        TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherMagazine",1,"WatchRaiseStartAmmo")
        goto WatchRaiseStartNoAmmo

      WatchRaiseStartNoAmmo:
        GE14 ABCDEFGHIJKLMNOPQRST 2 A_WeaponReady(WRF_NOBOB|WRF_NOFIRE|WRF_DISABLESWITCH)
        goto WatchRaiseFinish

      WatchRaiseStartAmmo:
        GE34 ABCDEFGHIJKLMNOPQRST 2 A_WeaponReady(WRF_NOBOB|WRF_NOFIRE|WRF_DISABLESWITCH)
        goto WatchRaiseFinish

      WatchRaiseFinish:
        TNT1 A 0 A_ZoomFactor(7.5)
        TNT1 A 0 A_GiveInventory("BondWatchUIInit")
        TNT1 A 0 A_GiveInventory("BondWatchToken")
        TNT1 A 0 ACS_NamedExecuteAlways("BondWatchInterface")
        TNT1 A 0 A_TakeInventory("BondWatchQueue")
        goto BackToReady
    }
}

// Attack handlers

actor SamsaraGoldenEyeRocketLauncherSingleAttackHandler : Trigger
{
    States
    {
      // Ammo checking/taking stuff
      Pickup:
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"PickupReloadable")
      PickupNonReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"PickupAttack")
        TNT1 A 0 A_JumpIfInventory("RocketAmmo",1,1)
        stop
        TNT1 A 0 A_TakeInventory("RocketAmmo",1)
        goto PickupAttack

      PickupReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,"PickupAttack")
        TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherMagazine",1,1)
        stop
        TNT1 A 0 A_TakeInventory("Goldeneye_RocketLauncherMagazine",1)
        goto PickupAttack

      // Attack stuff
      PickupAttack:
		TNT1 A 0 ACS_NamedExecuteWithResult("BondAltFireActivator")
        TNT1 A 0 A_JumpIfInventory("SamsaraAlternateSoundToken",1,"PickupAttackSource")
      // Original sounds
      PickupAttackN64:
        TNT1 A 0 A_PlaySound("Goldeneye/RocketLauncher",CHAN_WEAPON)
        TNT1 A 0 A_JumpIfInventory("SPModeOn",1,"PickupAttackN64Coop")
        TNT1 A 0 A_JumpIfInventory("CoopModeOn",1,"PickupAttackN64Coop")
      PickupAttackN64DM:
        TNT1 A 0 A_FireCustomMissile("Goldeneye_Rocket",frandom(-1,1),false,6,8,0,frandom(-3,3))
        stop

      PickupAttackN64Coop:
        TNT1 A 0 A_FireCustomMissile("Goldeneye_RocketCoop",frandom(-1,1),false,6,8,0,frandom(-3,3))
        stop

      // Source sounds
      PickupAttackSource:
        TNT1 A 0 A_PlaySound("GoldeneyeSource/RocketLauncher",CHAN_WEAPON)
        TNT1 A 0 A_JumpIfInventory("SPModeOn",1,"PickupAttackSourceCoop")
        TNT1 A 0 A_JumpIfInventory("CoopModeOn",1,"PickupAttackSourceCoop")
      PickupAttackSourceDM:
        TNT1 A 0 A_FireCustomMissile("Goldeneye_RocketSoundSource",frandom(-1,1),false,6,8,0,frandom(-3,3))
        stop

      PickupAttackSourceCoop:
        TNT1 A 0 A_FireCustomMissile("Goldeneye_RocketCoopSoundSource",frandom(-1,1),false,6,8,0,frandom(-3,3))
        stop
    }
}

