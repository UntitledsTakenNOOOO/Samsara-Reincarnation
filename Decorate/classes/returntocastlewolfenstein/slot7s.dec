Actor RTCW_EnemyBurnScreamer : Trigger
{
	States
	{
		Pickup:
			TNT1 A 0 ACS_NamedExecuteWithResult("GetActorGender")
			TNT1 A 0 A_JumpIfInventory("SamsaraEnemyGender",2,"ScreamFemale")
			TNT1 A 0 A_JumpIfInventory("SamsaraEnemyGender",1,"ScreamMale")
			Stop
		ScreamMale:
			TNT1 A 0 A_PlaySound("RTCW/Enemy/Burn",CHAN_7,1.0)
			Stop
		ScreamFemale:
			TNT1 A 0 A_PlaySound("RTCW/Enemy/BurnF",CHAN_7,1.0)
			Stop
	}
}