Actor Hexen2SoulSphere : CustomInventory
{
	Height 24
	Radius 12
	Renderstyle Translucent
	Alpha 0.6
	Inventory.PickupSound "Hexen2/ArtifactPickup"
	Inventory.PickupMessage "You have devoured life force"
	var int User_HealthToGive;
    States
    {
        Spawn:
			TNT1 A 0
            TNT1 A 0 A_SetUserVar("User_HealthToGive", 30)
            TNT1 A 0 A_SetSpecial(226, 5780, 0, User_HealthToGive, 0, 0)
        HealthLoop:
			TNT1 A 0 A_JumpIf(User_HealthToGive < 1, "Remove")
            HX00 A 17
			TNT1 A 0 A_SetUserVar("User_HealthToGive", User_HealthToGive - 1)
            TNT1 A 0 A_SetSpecial(226, 5780, 0, User_HealthToGive, 0, 0)
            Loop
		Remove:
			TNT1 A 0 A_SpawnItemEx("Hexen2LittleWhiteFlash",0,0,14)
			TNT1 A 1
			stop
		Pickup:
			TNT1 A 0 A_JumpIfInventory("ChexClass",1,"PickupHarm")
			TNT1 A 0 A_JumpIfInventory("HexenClass",1,"PickupHarm")
			TNT1 A 0 A_JumpIfInventory("Hexen2Class",1,"PickupHeal")
			TNT1 A 0 A_JumpIfInventory("CalebClass",1,"PickupHeal")
			Goto PickupFail
		PickupHeal:
			TNT1 A 0 A_RailWait
			stop
		PickupHarm:
			TNT1 A 0 A_RailWait
			stop
		PickupFail:
			TNT1 A 0 A_RailWait
			fail
    }
}

