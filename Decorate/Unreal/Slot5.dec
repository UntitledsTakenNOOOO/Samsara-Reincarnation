			UN02 A 2 A_TakeInventory("URocketsLoaded", 6)
			UN02 A 0 A_TakeInventory("Unreal_TargetLocked", 6)
			UN02 A 0 A_TakeInventory("Unreal_EightballLockTimer", 999)
			UN02 BCDE 2
