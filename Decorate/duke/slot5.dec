actor DukeRocket
{
    Radius 12
    Height 8
    Speed 30
    Damage (80)
    Projectile
    Obituary "$SAMSARA_DUKE_OB_SLOT5"
    Scale 0.55
    Decal Scorch
    DamageType "DukeExplosive"
    +FORCERADIUSDMG
    States
    {
      Spawn:
		RPGX A 5 bright	//Original Duke behaviour
	  SpawnLoop:
        RPGX A 1 bright A_SpawnItemEx("DukeRocketPuff",0,0,0,0,0,1.77,0,160)
        Loop

      Death:
        TNT1 A 0 A_Explode(128, 128)//(128, 128)
        TNT1 A 0 A_PlaySound("duke/explosion", 6, 1.0, 0, ATTN_NONE)
        TNT1 A 1 A_SpawnItemEx("RPGExplosion_2",0,0,0,0,0,0,0,160)
        stop
    }
}

