Script "DetermineValidSpawnItems" (void)
{
	// Check for Lexicon compatibility
	int LexiconTid = UniqueTid();
	
	if(SpawnForced("IKPuff",0,0,0,LexiconTid))
	{
		Thing_Remove(LexiconTid);
		Delay(5);
	}
	
	for(int a = 512; a <= 0.5; a+=1024)
	{
		for(int b = 512; b <= 0.5; b+=1024)
		{
			if(GetCVar("samsara_zscriptready") == 1)
				SpawnForced("DetermineValidCellItemsZS",a*1.0,b*1.0,0,0);
			else
				SpawnForced("DetermineValidCellItems",a*1.0,b*1.0,0,0);
			if(b > 0)
				b = -b - 1024;
			else
				b = -b;			
		}
		if(a > 0)
			a = -a - 1024;
		else
			a = -a;	
	}	
}

