actor EleGibs1
{
	radius 10
	height 10
	health 3
	speed 0
	gravity 0.2
	Scale 0.56
	-NOGRAVITY  
	//+FLOORCLIP
	+NOBLOCKMONST
	+MOVEWITHSECTOR  
	+SLIDESONWALLS
	+NOTELEPORT
	+CORPSE
	+NOBLOCKMAP  
	+RANDOMIZE
	+CLIENTSIDEONLY
	-SOLID
	+WINDTHRUST  
	-TELESTOMP
	+LOWGRAVITY
	States
	{
		Spawn:
			EGIB AB 5
			loop
		Crash:
			TNT1 A 0 A_PlaySound("Erad/BloodDrop")
			TNT1 A 0 A_Gravity
		Crash2:
			EGIB C 1000 A_SetFloorClip
			EGIB C 10 A_JumpIf(ACS_NamedExecuteWithResult("SamsaraClientDecorate", 13), "Crash2")
			stop
    }
}

Actor EradBloodDrop : EleGibs1
{
    States
    {
		Spawn:
			BLOS FEDCBA 4 A_Gravity
			loop
		Crash:
			TNT1 A 0 A_PlaySound("Erad/BloodDrop")	  
			BLSP ABCDEF 3
		Crash2:
			BLSP F 1000 A_SetFloorClip
			BLSP F 10 A_JumpIf(ACS_NamedExecuteWithResult("SamsaraClientDecorate", 13), "Crash2")
			stop
    }
}

actor EleArm
{
    Projectile
    Damage 0
    Speed 7
    BounceFactor 0.4
    health 1
	Scale 0.56
    -NOLIFTDROP
    -SOLID
	+RANDOMIZE
    +DOOMBOUNCE	
    +DROPOFF
//  +FLOORCLIP
    +NOBLOCKMONST	
    +MOVEWITHSECTOR	
    -NOGRAVITY
    +NOBLOCKMAP
    +CORPSE	
    +WINDTHRUST	
    +CLIENTSIDEONLY	
    -TELESTOMP
    +LOWGRAVITY	
    States
    {
		Spawn:
			EARM AB 4 A_Gravity
			Loop
        
		Death:
			TNT1 A 0 A_Gravity
		Death2:
			EARM C 1000 A_SetFloorClip
			TNT1 A 0 A_JumpIf(ACS_NamedExecuteWithResult("SamsaraClientDecorate", 13), "Death2")
			EARM C 1 A_FadeOut
			wait
    }
}

