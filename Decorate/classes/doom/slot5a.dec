	States
	{
	Spawn:
		64MI AA 2 bright A_SpawnItemEx("Doom64RocketSmokeTrail")
		Loop

	Death:
		64MI B 8 bright A_Explode
		64MI C 6 bright A_FadeOut(0.2)
		64MI D 3 bright A_FadeOut(0.2)
		64MI EF 3 bright A_FadeOut(0.2)
		Stop
	}
}

