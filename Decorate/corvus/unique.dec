        TNT1 A 0 A_FireCustomMissile("SamsaraCorvusEggFX",7.5,false,0,0,1)
        TNT1 A 0 A_FireCustomMissile("SamsaraCorvusEggFX",15,false,0,0,1)
        stop

      PDDMUse: // unused
        TNT1 A 0 A_FireCustomMissile("SamsaraCorvusEggFXPD",-15,false,0,0,1)
        TNT1 A 0 A_FireCustomMissile("SamsaraCorvusEggFXPD",-7.5,false,0,0,1)
