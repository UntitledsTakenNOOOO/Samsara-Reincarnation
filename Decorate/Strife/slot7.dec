actor " Mauler " : Mauler
{
  Weapon.Kickback 100
  +INVENTORY.UNDROPPABLE
  Weapon.SlotNumber 7
  Weapon.AmmoUse 10
  Weapon.AmmoGive 60
  Weapon.SelectionOrder 2800
  Weapon.AmmoType "Cell"
  +NOALERT
  Inventory.PickUpSound "strife/weaponget"
  Weapon.SisterWeapon ""
  Inventory.PickupMessage "You picked up the Mauler."
  Obituary "%k \citurned\c- %o \cito dust."
  Inventory.RestrictedTo "StrifeguyPlayer"  
  States
  {
  Ready:
    MAU1 A 0 A_JumpIfInventory("MaulerMode",1,"Ready2")
    MAU1 ABCD 6 A_WeaponReady
    Loop
  Deselect:
    MAU1 D 0 A_JumpIfInventory("MaulerMode",1,"Deselect2")
    MAU1 D 1 A_Lower
    Loop
  Select:
	MAU1 D 0 A_JumpIfInventory("MaulerMode",1,"Select2")
    MAU1 D 1 A_Raise
    Loop
  Fire:
    TNT1 A 0 A_AlertMonsters
    MAU1 D 0 A_JumpIfInventory("MaulerMode",1,"Fire2")
	MAUL A 0 Bright A_JumpIfInventory("CoopModeOn", 1, "CoopFire")
    MAUL A 5 Bright A_FireBullets(8.4,8.4,20,(random(2,3)*random(10,15)),"StrifeMaulerPuff",FBF_USEAMMO|FBF_NORANDOM)
	MAUL B 0 Bright A_PlayWeaponSound("strife/maulerfire1")
    MAUL B 3 Bright A_Light1
    MAUL C 2 A_Light2
    MAUL DE 2
    MAU1 D 7 A_Light0
    MAU1 A 7
    MAU1 B 7 A_CheckReload
    Goto Ready
  AltFire:
	MAU1 D 0 A_JumpIfInventory("MaulerMode",1,"AltFire2")
    MAU1 DDDDDDDDDDDD 1 A_Lower
    MAU1 A 0 A_GiveInventory("MaulerMode",1)
	MAU2 DDDDDDDDDDDD 1 A_Raise
	Goto Ready
  AltFire2:
    MAU2 DDDDDDDDDDDD 1 A_Lower
    MAU2 A 0 A_TakeInventory("MaulerMode",1)
	MAU1 DDDDDDDDDDDD 1 A_Raise
	Goto Ready
  CoopFire:
	MAUL AAAAAAAAAAAAAAAAAAA 0 A_FireCustomMissile("StrifeMaulerBullet", random(-840, 840)/100.00, 0, 0, 8, 0, random(-710, 710)/100.00)
	MAUL A 0 A_FireCustomMissile("StrifeMaulerBullet", random(-840, 840)/100.00, 1, 0, 8, 0, random(-710, 710)/100.00)
	MAUL A 5 Bright A_PlayWeaponSound("strife/maulerfire1")
    MAUL B 3 Bright A_Light1
    MAUL C 2 A_Light2
    MAUL DE 2
    MAU1 D 7 A_Light0
    MAU1 A 7
    MAU1 B 7 A_CheckReload
    Goto Ready
  Ready2:
    MAU2 ABCD 7 A_WeaponReady
    Loop
  Deselect2:
    MAU2 D 1 A_Lower
    Loop
  Select2:
    MAU2 D 1 A_Raise
    Loop
  Fire2:
    TNT1 A 0 A_AlertMonsters
    MAU2 A 0 A_JumpIfInventory("Cell",30,30)
	MAU2 A 0 A_JumpIfInventory("Cell",29,"AltFire2")
	MAU2 A 0 A_JumpIfInventory("Cell",28,"AltFire2")
	MAU2 A 0 A_JumpIfInventory("Cell",27,"AltFire2")
	MAU2 A 0 A_JumpIfInventory("Cell",26,"AltFire2")
	MAU2 A 0 A_JumpIfInventory("Cell",25,"AltFire2")
	MAU2 A 0 A_JumpIfInventory("Cell",24,"AltFire2")
	MAU2 A 0 A_JumpIfInventory("Cell",23,"AltFire2")
	MAU2 A 0 A_JumpIfInventory("Cell",22,"AltFire2")
	MAU2 A 0 A_JumpIfInventory("Cell",21,"AltFire2")
	MAU2 A 0 A_JumpIfInventory("Cell",20,"AltFire2")
	MAU2 A 0 A_JumpIfInventory("Cell",19,"AltFire2")
	MAU2 A 0 A_JumpIfInventory("Cell",18,"AltFire2")
	MAU2 A 0 A_JumpIfInventory("Cell",17,"AltFire2")
	MAU2 A 0 A_JumpIfInventory("Cell",16,"AltFire2")
	MAU2 A 0 A_JumpIfInventory("Cell",15,"AltFire2")
	MAU2 A 0 A_JumpIfInventory("Cell",14,"AltFire2")
	MAU2 A 0 A_JumpIfInventory("Cell",13,"AltFire2")
	MAU2 A 0 A_JumpIfInventory("Cell",12,"AltFire2")
	MAU2 A 0 A_JumpIfInventory("Cell",11,"AltFire2")
	MAU2 A 0 A_JumpIfInventory("Cell",10,"AltFire2")
