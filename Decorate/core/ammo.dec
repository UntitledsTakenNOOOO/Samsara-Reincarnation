      Pickup:
        TNT1 A 0 A_JumpIfInventory("QuakeClass",1,"PickupQuake1")
        TNT1 A 0 A_JumpIfInventory("RottClass",1,"PickupROTT")
        TNT1 A 0 A_JumpIfInventory("DisruptorClass",1,"PickupDisruptor")
