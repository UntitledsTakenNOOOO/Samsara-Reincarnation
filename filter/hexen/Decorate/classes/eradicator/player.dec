actor EradicatorDukeTauntTrigger : Trigger
{
    States
    {
      Pickup:
        TNT1 A 0 A_JumpIfInventory("EradMode",1,"PickupMale")
      PickupFemale:
        TNT1 A 0 A_GiveInventory("DukeWomenTaunt",1,AAPTR_TARGET)
        goto PickupFinish

      PickupMale:
        TNT1 A 0 A_GiveInventory("DukeDMTaunt1",1,AAPTR_TARGET)
        goto PickupFinish

      PickupFinish:
        TNT1 A 0 A_RailWait
        stop
    }
}
